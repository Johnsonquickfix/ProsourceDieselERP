
@{
    ViewBag.Title = "Business Snapshot";
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.rangePicker = "/Content/AdminLTE-3.1.0/bower_components/bootstrap-daterangepicker/daterangepicker.css";
}
<style>
    .highcharts-data-table table {
        border-collapse: collapse;
        border-spacing: 0;
        background: white;
        min-width: 100%;
        margin-top: 10px;
        font-family: sans-serif;
        font-size: 0.9em;
    }

    .highcharts-data-table td, .highcharts-data-table th, .highcharts-data-table caption {
        border: 1px solid silver;
        padding: 0.5em;
    }

    .highcharts-data-table tr:nth-child(even), .highcharts-data-table thead tr {
        background: #f8f8f8;
    }

    .highcharts-data-table tr:hover {
        background: #eff;
    }

    .highcharts-data-table caption {
        border-bottom: none;
        font-size: 1.1em;
        font-weight: bold;
    }

    .highcharts-data-table th[scope="col"] {
        text-decoration: underline;
        cursor: pointer;
    }
</style>
<div id="loader" class="d-flex align-items-center">
    <img src="data:image/gif;base64," align="">
    <span>&nbsp;&nbsp;Loading......</span>
</div>
<div class="page-title">
    <h2>Business Snapshot</h2>
</div>
<section class="content section-content neworder-section table-design">
    <div class="box">
        <div class="box-body">
            <div class="row mb-1">
                <div class="col-md-4">
                    <div class="input-group">
                        <span class="input-group-addon"><i class="fa fa-calendar"></i></span>
                        <input id="txtDate" data-auto-update-input="true" readonly="readonly" class="form-control">
                    </div>
                </div>
                <div class="col-md-8">
                    <button type="button" title="Click here to print" data-placement="left" data-toggle="tooltip" id="btnSearch" class="btn btn-primary btn-flat button">Search</button>
                    <button type="button" title="Click here to export Profit Loss Account" data-placement="top" data-toggle="tooltip" id="btnExportDetails" class="btn btn-primary btn-flat button">Export Details</button>
                    <button type="button" title="Click here to print" data-placement="left" data-toggle="tooltip" id="btnChange" onclick="printDiv();" class="btn btn-primary btn-flat button pull-right">Print</button>
                </div>
            </div>
        </div>
    </div>
    <div class="row mb-1">
        <div class="col-md-6">
            <div class="box">
                <div class="box-body">
                    <div id="pie_chart_income"></div>
                </div>
            </div>
        </div>
        <div class="col-md-6">
            <div class="box">
                <div class="box-body">
                    <div id="pie_chart_expence"></div>
                </div>
            </div>
        </div>
    </div>
    <div class="row mb-1">
        <div class="col-md-6">
            <div class="box">
                <div class="box-body">
                    <div id="bar_comp_income"></div>
                </div>
            </div>
        </div>
        <div class="col-md-6">
            <div class="box">
                <div class="box-body">
                    <div id="bar_comp_expence"></div>
                </div>
            </div>
        </div>
    </div>
</section>
<script src="~/js/linq.js"></script>
<script src="~/Content/AdminLTE-3.1.0/jquery/jquery.min.js"></script>
<link href="~/Content/AdminLTE-3.1.0/daterangepicker/daterangepicker.monthpicker.css" rel="stylesheet" />
<script src="~/Content/AdminLTE-3.1.0/daterangepicker/daterangepicker.monthpicker.js"></script>
<script src="~/Content/highcharts/highcharts.js"></script>
<script src="~/Content/highcharts/series-label.js"></script>
<script src="~/Content/highcharts/data.js"></script>
<script src="~/Content/highcharts/exporting.js"></script>
<script src="~/Content/highcharts/export-data.js"></script>
<script src="~/Content/highcharts/accessibility.js"></script>


<script type="text/javascript">
    $(document).ready(function () {
        $("#loader").hide();
        $('#txtDate').daterangepicker({
            ranges: {
                'Today': [moment(), moment()],
                'Yesterday': [moment().subtract(1, 'days'), moment().subtract(1, 'days')],
                'Last 7 Days': [moment().subtract(6, 'days'), moment()],
                'Last 30 Days': [moment().subtract(29, 'days'), moment()],
                'This Month': [moment().startOf('month'), moment().endOf('month')],
                'Last Month': [moment().subtract(1, 'month').startOf('month'), moment().subtract(1, 'month').endOf('month')],
                'This Year': [moment().startOf('year'), moment().endOf('year')]
            },
            startDate: moment().startOf('year'), autoUpdateInput: true, autoApply: true, alwaysShowCalendars: true,
            locale: { format: 'MM/DD/YYYY', cancelLabel: 'Clear' }, opens: 'right', orientation: "left auto"
        }, function (start, end, label) {
            $('#txtDate').val(start.format('MM/DD/YYYY') + ' - ' + end.format('MM/DD/YYYY'));
            //$.when(GetOrderDetails()).done(function () { dataGridLoad(order_type); });
        });
        $.when(getProfitLossAccounts()).done(function () { getYearlyComparison();});
        //$(document).on("change", "#ddlSearchBy", function (t) {
        //    t.preventDefault(); let id = parseInt($("#ddlSearchBy").val()) || 0; SearchByControl(id)
        //});
        //$('#ddlfinaceyear').change(function () { getProfitLoss(); });
        //$(document).on("click", "#btnSearch", function (t) { t.preventDefault(); getProfitLoss(); });
        //$(document).on("click", "#btnExportDetails", function (t) { t.preventDefault(); ExportProfitLoss(); });
    });
    function formatCurrency(total) {
        var neg = false;
        if (total < 0) { neg = true; total = Math.abs(total); }
        return (neg ? "-$" : '$') + parseFloat(total, 10).toFixed(2).replace(/(\d)(?=(\d{3})+\.)/g, "$1,").toString();
    }
    function getProfitLossAccounts() {
        let _date_from = $('#txtDate').data('daterangepicker').startDate, _date_to = $('#txtDate').data('daterangepicker').endDate;
        let option = { fiscalyear_id: 0, from_date: _date_from.format('MM-DD-YYYY'), to_date: _date_to.format('MM-DD-YYYY'), report_type: 'IEREPORT' };
        $.ajax({
            url: "/accounting/get-income-expence", dataType: 'json', contentType: "application/json; charset=utf-8", data: option, beforeSend: function () { $("#loader").show(); },
            success: function (result) {
                try {
                    result = JSON.parse(result);
                    let _income = Enumerable.from(result).where(function (e) { return (e.extraparams === "I" && e.amount != 0); }).select(function (e) { return { name: e.label, y: Math.round(e.amount * 100) / 100 }; }).toArray();
                    let _expence = Enumerable.from(result).where(function (e) { return (e.extraparams === "E" && e.amount != 0); }).select(function (e) { return { name: e.label, y: Math.round(e.amount * 100) / 100 }; }).toArray();
                    bindPieChart(_income, _expence);
                }
                catch (error) { throw error; }
            },
            complete: function () { $("#loader").hide(); },
            error: function (xhr, status, err) { $("#loader").hide(); swal('Alert!', "something went wrong.", "error"); }, async: true
        });
    }
    function bindPieChart(_income, _expence) {
        Highcharts.setOptions({ lang: { decimalPoint: '.', thousandsSep: ',' } });
        Highcharts.chart('pie_chart_income', {
            //colors: ['#2f7ed8', '#0d233a', '#8bbc21', '#910000', '#1aadce', '#492970', '#f28f43', '#77a1e5', '#c42525', '#a6c96a'],
            chart: { plotBackgroundColor: null, plotBorderWidth: null, plotShadow: false, type: 'pie' },
            title: { text: 'Income' }, 
            tooltip: { pointFormat: '{series.name}: {point.y:,.2f} <b>({point.percentage:.1f}%)</b>' },
            accessibility: { point: { valueSuffix: '%' } },
            plotOptions: { pie: { allowPointSelect: true, cursor: 'pointer', showInLegend: false, dataLabels: { enabled: true, format: '<b>{point.name}</b>: {point.y:,.2f} ({point.percentage:.1f}%)' } } },
            series: [{ name: 'Income', colorByPoint: true, data: _income }]
        });

        Highcharts.chart('pie_chart_expence', {
            chart: { plotBackgroundColor: null, plotBorderWidth: null, plotShadow: false, type: 'pie' },
            title: { text: 'Expence' },
            tooltip: { pointFormat: '{series.name}: ({point.y:,.2f}) <b>({point.percentage:.1f}%)</b>' },
            accessibility: { point: { valueSuffix: '%' } },
            plotOptions: { pie: { allowPointSelect: true, cursor: 'pointer', showInLegend: false, dataLabels: { enabled: true, format: '<b>{point.name}</b>: {point.y:,.2f} ({point.percentage:.1f}%)' } } },
            series: [{ name: 'Expence', colorByPoint: true, data: _expence }]
        });

        $('.highcharts-credits').remove();
    }

    function getYearlyComparison() {
        let _date_from = $('#txtDate').data('daterangepicker').startDate, _date_to = $('#txtDate').data('daterangepicker').endDate;
        let option = { fiscalyear_id: 0, from_date: _date_from.format('MM-DD-YYYY'), to_date: _date_to.format('MM-DD-YYYY'), report_type: 'PYIEC' };
        $.ajax({
            url: "/accounting/get-income-expence", dataType: 'json', contentType: "application/json; charset=utf-8", data: option, beforeSend: function () { $("#loader").show(); },
            success: function (result) {
                try {
                    result = JSON.parse(result);
                    let _income = Enumerable.from(result).where(function (e) { return (e.extraparams === "I" && e.amount != 0); }).select(function (e) { return { name: e.label, y: Math.round(e.amount * 100) / 100 }; }).toArray();
                    let _expence = Enumerable.from(result).where(function (e) { return (e.extraparams === "E" && e.amount != 0); }).select(function (e) { return { name: e.label, y: Math.round(e.amount * 100) / 100 }; }).toArray();
                    //bindPieChart(_income, _expence);
                }
                catch (error) { throw error; }
            },
            complete: function () { $("#loader").hide(); },
            error: function (xhr, status, err) { $("#loader").hide(); swal('Alert!', "something went wrong.", "error"); }, async: true
        });
    }
</script>