
@{
    ViewBag.Title = "DamageProductList";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<link href="~/Content/AdminLTE-3.1.0/datatables/DataTables-1.10.20/css/dataTables.bootstrap.min.css" rel="stylesheet" />
<link href="~/Content/AdminLTE-3.1.0/datatables/Buttons-1.6.1/css/buttons.dataTables.min.css" rel="stylesheet" />
@*datatables js*@
<script src="~/Content/AdminLTE-3.1.0/datatables/datatables.min.js" type="text/javascript"></script>
<script src="~/Content/AdminLTE-3.1.0/datatables/DataTables-1.10.20/js/dataTables.bootstrap.min.js" type="text/javascript"></script>

<div class="page-title">
    <h2 class="page-heading">Damage stock</h2><a class="btn btn-danger" href="../Warehouse/DamageProduct" id="btnAddAccount"><span title="Click here to add damage product" data-placement="bottom" data-toggle="tooltip">Add Damage Product</span></a>
</div>
<section class="content section-content table-design customer-section">

    <div class="row">
        <div class="userrole_section section_wrapper appearance-menu-section">
            @*<form id="saveForm" name="saveForm" method="post" autocomplete="off">*@
            <div class="box box-primary">
                <div class="box-header">
                    <table id="DamageProductData" class="table-blue table check-table table-bordered table-striped dataTable " width="100%">
                        <thead>
                            <tr>
                                <th>Ref</th>
                                <th>Order/Purchase/Return ID</th>
                                <th>Product</th>
                                <th>Vendor</th>
                                <th>Warehouse</th>
                                <th>Return reason</th>
                                <th>Date</th>
                                <th>Qty</th>
                                <th>Action</th>
                            </tr>
                        </thead>
                    </table>
                </div>
            </div>
            @*</form>*@
        </div>
    </div>
</section>

<script src="@Url.Content("~/js/Warehouse/DamageProduct.js?v="+DateTime.Now.ToString("ddMMyyyymmss")+"")" asp-append-version="true"></script>
