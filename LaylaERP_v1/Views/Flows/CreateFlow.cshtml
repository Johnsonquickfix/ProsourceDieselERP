    Layout = "~/Views/Shared/_qfk_Layout.cshtml";
}
<link href="~/Content/qfk/css/flow-layout.css" rel="stylesheet" />
<link href="~/Content/qfk/Selectr/selectr.min.css" rel="stylesheet" />
<script src="~/Content/qfk/Selectr/selectr.min.js"></script>
<style>
    .configuration-panel .form-control {
        height: 34px;
        padding: 5px 8px;
        border: 1px solid rgb(145, 151, 159);
        border-radius: 2px;
    }

    .fa-envelope-circle-check:before {
        content: "\e4e8";
    }
</style>
<div id="root" data-id="@ViewBag.id">
    <div class="flows-body">
