
@{
    ViewBag.Title = "Profiles";
    Layout = "~/Views/Shared/_qfk_Layout.cshtml";
}

<link href="~/Content/AdminLTE-3.1.0/datatables/DataTables-1.10.20/css/dataTables.bootstrap4.min.css" rel="stylesheet" />
<link href="~/Content/AdminLTE-3.1.0/datatables/Buttons-1.6.1/css/buttons.dataTables.min.css" rel="stylesheet" />
<!-- datatables js -->
<script src="~/Content/AdminLTE-3.1.0/datatables/datatables.min.js" type="text/javascript"></script>
<script src="~/Content/AdminLTE-3.1.0/datatables/DataTables-1.10.20/js/dataTables.bootstrap4.min.js" type="text/javascript"></script>

<!-- start page title -->
<div class="row">
    <div class="col-12">
        <div class="page-title-box d-sm-flex align-items-center justify-content-between">
            <div>
                <h4 class="mb-sm-0 font-size-18">Profiles</h4>
                <p class="text-muted mb-0">Profile counts: <span class="profile-counts">0</span></p>
                

                </div>
            <div class="page-title-right">
                <a class="btn btn-danger" href="../../../Customer/Customer"><span title="Click here to back to customer's list" data-placement="bottom" data-toggle="tooltip">Back to List</span></a>
                <div class="active text-right" style="margin-left:auto;min-width:280px;">
                    <label id="lblemail" style="display:none" for="file">@Model.user_email</label>
                    <label id="lblid" style="display:none" for="file">@Model.ID</label>
                    <ol class="breadcrumb m-0">
                        <li class="breadcrumb-item"><a href="../../profiles/search-id/@Model.ID">Audience</a></li>
                        <li class="breadcrumb-item active">Profiles</li>
                    </ol>
                </div>

            </div>
    </div>
</div>
<!-- end page title -->
<div class="row">
    <div class="col-12">
        <div class="card">
            <div class="card-body">
                <table id="datatable" class="table dt-responsive w-100 align-middle">
                </table>
            </div>
        </div>
    </div>
</div>
<script async src="@Url.Content("~/js/qfk/audience/profilesbyid.js?v=" + DateTime.Now.ToString("ddMMyyyymmss"))" asp-append-version="true"></script>
