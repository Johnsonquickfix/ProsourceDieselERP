<script>
    $(document).ready(function () {
        $('#loader').hide();
        dateRange('#DateRange');
        dateRange('#DateRange2');
        $('#DateRange2').val('');
        $('#DateRange2').on('cancel.daterangepicker', function (ev, picker) { $(this).val(''); });
        gettotalOrder();
        getTotalSales();
        gettotalCustomer();
        getCompletedOrder();
        loadData();

        gettotal('#DateRange');
        $('#DateRange').change(function () { loadData(); gettotal('#DateRange'); });
        $('#DateRange2').change(function () {
            var fromdate = $('#DateRange2').data('daterangepicker').startDate.format('MM-DD-YYYY');
            var todate = $('#DateRange2').data('daterangepicker').endDate.format('MM-DD-YYYY');
            $('#DateRange').val($('#DateRange2').val());
            loadData(); gettotalOrder(fromdate, todate);
            getTotalSales(fromdate, todate);
            gettotalCustomer(fromdate, todate);
            getCompletedOrder(fromdate, todate);
            gettotal('#DateRange2');
            drawChart(fromdate, todate); //Google sales chart
            Search(fromdate, todate); //High chart sales
        });
        $('#sendEmail').click(function () { emails(); });
        //High chart start
        Search();
        $(function () {
            Highcharts.setOptions({
                lang: {
                    thousandsSep: ','
                }
            });
        }) //High chart end

    });
    function dateRange(dtr) {
        $(dtr).daterangepicker({
            ranges: {
                'Today': [moment(), moment()],
                'Yesterday': [moment().subtract(1, 'days'), moment().subtract(1, 'days')],
                'Last 7 Days': [moment().subtract(6, 'days'), moment()],
                'Last 30 Days': [moment().subtract(29, 'days'), moment()],
                'This Month': [moment().startOf('month'), moment().endOf('month')],
                'Last Month': [moment().subtract(1, 'month').startOf('month'), moment().subtract(1, 'month').endOf('month')]
            },
            startDate: moment(),
            autoUpdateInput: true, alwaysShowCalendars: true,
            locale: { format: 'MM/DD/YYYY', cancelLabel: 'Clear' },
            opens: 'left',
            orientation: "right auto",
        });
    }
    function loadData() {
        $("#loader").show();
        $.post("/Home/GetOrderList", $("#saveForm").serialize(), function (response) {
            $("#content").html(response);
            $("#loader").hide();
        });
    }
    function gettotal(dtr) {
        var fromdate = $(dtr).data('daterangepicker').startDate.format('MM-DD-YYYY');
        var todate = $(dtr).data('daterangepicker').endDate.format('MM-DD-YYYY');
        var obj = { strValue5: fromdate, strValue6: todate };
        $.ajax({
            url: '/Home/GettotalDetsils/',
            type: 'post',
            contentType: "application/json; charset=utf-8",
            dataType: 'JSON',
            data: JSON.stringify(obj),
            success: function (data) {
                var i = JSON.parse(data);
                if (parseInt(i[0].TotalOrder) > 10)
                    $("#lblorder").text('10');
                else
                    $("#lblorder").text(i[0].TotalOrder);
                $("#lblsale").text(parseFloat(i[0].TotalSale).toFixed(2));
                $("#lblordercounting").text(i[0].TotalOrder);
                //$("#content").html(response);
            },
            error: function (msg) { alert(msg); }
        });
    }
    function gettotalOrder(fromdate, todate) {
        var obj = { strValue5: fromdate, strValue6: todate };
        $.ajax({
            url: '/Home/GettotalOrders/',
            type: 'post',
            contentType: "application/json; charset=utf-8",
            dataType: 'JSON',
            data: JSON.stringify(obj),
            beforeSend: function () { $("#loader").show(); },
            success: function (data) {
                $("#lbltotalOrder").text(data.data);
            },
            error: function (msg) { alert(msg); },
            complete: function () { $("#loader").hide(); }
        });
    }
    function getTotalSales(fromdate, todate) {

        var obj = { strValue5: fromdate, strValue6: todate };
        $.ajax({
            url: '/Home/GettotalSales/',
            type: 'post',
            contentType: "application/json; charset=utf-8",
            dataType: 'JSON',
            data: JSON.stringify(obj),
            beforeSend: function () { $("#loader").show(); },
            success: function (data) {
                $("#lbltotalSales").text('$' + data.data);
            },
            error: function (msg) { alert(msg); },
            complete: function () { $("#loader").hide(); }
        });
    }
    function gettotalCustomer(fromdate, todate) {
        var obj = { strValue5: fromdate, strValue6: todate };
        $.ajax({
            url: '/Home/GettotalCustomer/',
            type: 'post',
            contentType: "application/json; charset=utf-8",
            dataType: 'JSON',
            data: JSON.stringify(obj),
            beforeSend: function () { $("#loader").show(); },
            success: function (data) {
                $("#lbltotalCustomer").text(data.data);

            },
            error: function (msg) { alert(msg); },
            complete: function () { $("#loader").hide(); }
        });
    }
    function getCompletedOrder(fromdate, todate) {
        var obj = { strValue5: fromdate, strValue6: todate };
        $.ajax({
            url: '/Home/GetcompletedOrders/',
            type: 'post',
            contentType: "application/json; charset=utf-8",
            dataType: 'JSON',
            data: JSON.stringify(obj),
            beforeSend: function () { $("#loader").show(); },
            success: function (data) {
                $("#lblCompletedCustomer").text(data.data);
            },
            error: function (msg) { alert(msg); },
            complete: function () { $("#loader").hide(); }
        });
    }
    function emails() {
        let emails = $('#emailto').val();
        let subject = $('#subject').val();
        let content = $('#content1').val();

        let isVal = 1;
        if (emails == "") {
            swal('Alert', 'Please enter email address.', 'error').then(function () { swal.close(); $('#emailto').focus(); });
        }
        else if (subject == "") {
            swal('Alert', 'Please enter subject.', 'error').then(function () { swal.close(); $('#subject').focus(); });
        }
        else if (content == "") {
            swal('Alert', 'Please enter message.', 'error').then(function () { swal.close(); $('#content1').focus(); });
        }
        else {
            var obj = { emails: emails, subject: subject, content: content }
            $.ajax({
                url: '/AccountAPI/QuicksendEmail/',
                type: 'post',
                contentType: "application/json; charset=utf-8",
                dataType: 'JSON',
                data: JSON.stringify(obj),
                beforeSend: function () { $("#loader").show(); },
                success: function (data) {
                    //$("#loading-div-background").hide();
                    swal('success!', "Email send Successfully", "success").then(function () {
                        $('#emailto,#subject').val('');
                        // remove textarea value
                        $('.wysihtml5-sandbox, .wysihtml5-toolbar').remove();
                        $("#content1").wysihtml5({ toolbar: { "link": false, "image": false, } });
                        $('#content1').show();
                        $('#content1').val('');
                        //end textarea remove
                    });


                },
                error: function (errormessage) { $("#loader").hide(); swal('Error!', "Something went wrong .", "error"); },
                complete: function () { $("#loader").hide(); }
            });
        }
    }
</script>

