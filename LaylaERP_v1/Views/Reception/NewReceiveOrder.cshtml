
@{
    ViewBag.Title = "New Receive Order";
    Layout = "~/Views/Shared/_Layout.cshtml";
    var user = LaylaERP.UTILITIES.CommanUtilities.Provider.GetCurrent();
}
<div id="loader" class="d-flex align-items-center">
    <img src="data:image/gif;base64," align="">
    <span>&nbsp;&nbsp;Loading......</span>
</div>
<div class="page-title">
    <h2 class="page-heading" id="lbltitle">Receive Order</h2>
    <div class="top-action text-right" style="margin-left:auto;min-width:280px;">
    </div>
    <button type="button" style="display:none" class="btn btn-danger hidden" id="btnPrintPdf"><i class="fas fa-print"></i> Print</button>
</div>
<div id="divAlert">
</div> 
<!-- Main content -->
<section class="content content-section p-0">
    <div class="box box-primary">
        <div class="box-body">
            <div class="form-horizontal create-new-customer" id="parent" data-com="@user.CompanyName" data-ad1="@user.address" data-ad2="@user.address1" data-city="@user.City" data-state="@user.State" data-country="@user.Country" data-zip="@user.postal_code" data-email="@user.email" data-website="@user.website">
                <div class="row">
                    <div class="col-md-4">
                        <div class="form-group">
                            <label>Date<span class="text-red">*</span></label>
                            <div class="input-group">
                                <input type="hidden" id="hfid" />
                                <input type="hidden" id="hfstatus" />
                                <input type="hidden" id="hftext" />
                                <input type="hidden" id="hfsaleno" />
                                <span class="input-group-addon"><i class="fa fa-calendar"></i></span>
                                <input id="txtPODate" readonly="readonly" disabled class="form-control">
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label" for="Ref">PO No.</label>
                            <div class="">
                                <label class="control-label" for="Ref" id="lblPoNo" data-id="@ViewBag.id">Draft</label>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label" for="Vendor" data-toggle="tooltip" title="Select Vendor for creating Purchase Order.">Vendor<span class="text-red">*</span></label>
                            <div class="">
                                <select class="form-control select2" id="ddlVendor"></select>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label " for="Ref. vendor" data-toggle="tooltip" title="Vendor Bill/Ref. No">Ref. vendor</label>
                            <div class="">
                                <input type="text" id="txtRefvendor" class="form-control billinfo" placeholder="Ref. vendor" />
                            </div>
                        </div>

                    </div>
                    <div class="col-md-4">
                        <div class="form-group">
                            <label class="control-label " for="Payment Terms">Payment Terms</label>
                            <div class="">
                                <select class="form-control select2 billinfo" id="ddlPaymentTerms"></select>
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="Balance">Balance days</label>
                            <select class="form-control select2 billinfo" id="ddlBalancedays"></select>
                        </div>
                        <div class="form-group">
                            <label class="control-label" for="Incoterms">Incoterms</label>
                            <div class="input-group">
                                <select class="form-control select2 billinfo" id="ddlIncoTerms" placeholder="Incoterms" style="width: 100%;"></select>
                                <div class="input-group-btn" style="width:45%">
                                    <input type="text" id="txtIncoTerms" class="form-control input-group-text billinfo" placeholder="IncoTerms" />
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label" for="ddlPaymentType" data-toggle="tooltip" title="Vendor payment type.">Payment Type<span class="text-red">*</span></label>
                            <div class="">
                                <select class="form-control select2 billinfo" id="ddlPaymentType"></select>
                            </div>
                        </div>

                    </div>
                    <div class="col-md-4">
                        <div style="display:none" class="form-group">
                            <label class="control-label" for="ddlwarehousepo" data-toggle="tooltip" title="Warehouse .">Warehouse PO <span class="text-red">*</span></label>
                            <div class="">
                                <select class="form-control select2 billinfo" id="ddlwarehousepo"></select>
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="control-label " for="Planned date of delivery" data-toggle="tooltip" title="Planned date of delivery by vendor.">Planned date of delivery<span class="text-red">*</span></label>
                            <div class="">
                                <div class="input-group">
                                    <span class="input-group-addon"><i class="fa fa-calendar"></i></span>
                                    <input type="text" id="txtPlanneddateofdelivery" class="form-control billinfo" placeholder="Planned date of delivery (MM/DD/YYYY)" autocomplete="off" />
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label " for="Note">Note</label>
                            <div class="">
                                <textarea id="txtNotePublic" class="form-control billinfo" placeholder="Note"></textarea>
                            </div>
                        </div>
                        <div style="display:none" class="form-group">
                            <label class="control-label " for="Note (private)">Note (private)</label>
                            <div class="">
                                <textarea id="txtNotePrivate" class="form-control billinfo" placeholder="Note (private)"></textarea>
                            </div>
                        </div>
                        <label class="control-label" for="ddlwarehouse" data-toggle="tooltip" title="Warehouse .">Warehouse Receive<span class="text-red">*</span></label>
                        <div class="">
                            <select class="form-control billinfo" id="ddlwarehouse"></select>
                        </div>
                    </div>

                </div>
            </div>
        </div>
    </div>
    `    <!--table start-->
    <div class="row">
        <div class="col-sm-12">
            <div class="box box-primary">
                <div class="box-header">
                    <div class="table-responsive" id="divAddItemFinal">
                        <table id="tblAddItemFinal" class="table-blue table table-bordered table-striped dataTable">
                            <thead>
                                <tr>
                                    <th class="text-center" style="width: 8%">Actions</th>
                                    <th style="width: 26%">Description</th>
                                    <th style="width: 10%">Vendor SKU</th>
                                    <th class="text-left" style="width: 10%">Price</th>
                                    <th class="text-left" style="width: 10%">Ordered</th>
                                    <th class="text-left" style="width: 10%">Received</th>
                                    <th class="text-left" style="width: 10%">Remaining</th>
                                    <th class="text-left" style="width: 10%">To Receive</th>
                                    <th class="text-left" style="width: 10%">Discount(%)</th>
                                    <th class="text-right" style="display:none">Tax</th>
                                    <th class="text-right" style="display:none">Shipping</th>
                                    <th class="text-right" style="width: 10%">Total</th>
                                    @*<th class="text-right" style="width: 10%">Avg.</th>*@
                                </tr>
                            </thead>
                            <tbody id="line_items"></tbody>
                            <tbody id="product_line_items"></tbody>
                            <tfoot class="thead-dark">
                                <tr>
                                    <th colspan="4" class="text-left" style="width: 58%">
                                        <strong>Total <span id="Totalqty">0</span></strong>
                                    </th>
                                    <th class="text-left" style="width: 8%">
                                        <strong><span id="Totalorder">0 </span></strong>
                                    </th>
                                    <th class="text-left" style="width: 8%">
                                        <strong><span id="Totalrecived">0 </span></strong>
                                    </th>
                                    <th class="text-left" style="width: 8%">
                                        <strong><span id="Totalremanig">0 </span></strong>
                                    </th>
                                    <th class="text-left" style="width: 37%" colspan="5">
                                        <strong><span id="Totalbalc">0 </span></strong>
                                    </th>
                                </tr>
                            </tfoot>
                        </table>

                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-sm-6">
            <div class="box box-primary">
                <div class="box-header with-border">
                    <h3 class="box-title">Received History </h3>
                </div>
                <div class="box box-solid">
                    <div class="table-responsive" id="divhistory">
                        <table id="tblhistory" class="table-blue table table-bordered table-striped dataTable">
                            <thead class="theadcss">
                                <tr>
                                    <th class="text-left" style="width: 20%">Description</th>
                                    <th class="text-left" style="width: 12%">Date</th>
                                    <th class="text-left" style="width: 10%">Qty</th>
                                    @*<th class="text-left" style="width: 15%">Discount</th>*@
                                    <th class="text-left" style="width: 13%">Amount</th>
                                    <th class="text-left" style="width: 15%">Shipping</th>
                                    <th class="text-left" style="width: 10%">Tax</th>
                                    <th class="text-left" style="width: 10%">Avg.</th>
                                </tr>
                            </thead>
                            <tbody class="tbodycss" id="product_history"></tbody>
                        </table>

                    </div>
                    @*<div class="box-header with-border">
                        <h3 class="box-title">Files</h3>
                    </div>
                    <div class="box-body">
                        <ul class="nav nav-pills nav-stacked" id="billfiles">
                            <li>None</li>
                        </ul>
                    </div>*@
                </div>
            </div>

            <div class="box box-primary">
                <div class="box-header with-border">
                    <h3 class="box-title">Received Invoice History</h3>
                </div>
                <div class="box box-solid">
                    <div class="table-responsive" id="divinvoichistory">
                        <table id="tblinvoichistory" class="table-blue table table-bordered table-striped dataTable">
                            <thead class="theadcss">
                                <tr>
                                    <th class="text-left" style="width: 15%">Bill No</th>
                                    <th class="text-left" style="width: 15%">Receive Date</th>
                                    <th class="text-left" style="width: 30%">Description</th>
                                    <th class="text-left" style="width: 15%">Quantity</th>
                                    <th class="text-left" style="width: 20%">Amount</th>
                                </tr>
                            </thead>
                            <tbody class="tbodycss" id="product_invoichistory"></tbody>
                        </table>
                    </div>
                </div>
            </div>

            <div class="row rowbox proposal-row">
                <div class="col-md-12">
                    <div class="box box-primary">
                        <div class="box-header with-border">
                            <label class="control-label">Attach a new file/document</label>
                            <div class="col-sm-12">
                                <div class="form-group">
                                    <div class="col-md-6">
                                        <input type="file" name="ImageFile" id="ImageFile" />
                                    </div>
                                    <div class="col-md-6 left">
                                        <input type="button" class="billinfofo" id="btnuploade" value="Upload" />
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="box-body">
                            <div class="mb-35">
                                <label class="control-label">Attached files and documents</label>
                                <div class="table-responsive bank-table-wrapper">
                                    <div class="table-responsive" id="divfileupload">
                                        <table id="dtfileupload" class="table-blue table check-table table-bordered table-striped dataTable " width="100%">
                                            <thead>
                                                <tr>
                                                    <th>Documents</th>
                                                    <th>Size</th>
                                                    <th>Date</th>
                                                    @*<th>View</th>*@
                                                    <th class="text-right">Delete</th>
                                                </tr>
                                            </thead>
                                            <tbody id="divfileupload_services"></tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>


        </div>
        
        <div class="col-sm-6 order-box-total">
            <div class="box box-primary">
                <div class="row">
                     
                    <div class="col-sm-6">
                        <div class="box-body">
                            <div class="form-group">
                                <label class="col-sm-10 control-label text-left">Shipping/Freight</label>
                                <div>
                                    <input min="0" autocomplete="off" value="0" class="text-right form-control billinfofo number " type="number" id="txtshippingfee" placeholder="Shipping Fee" />
                                </div>
                            </div>

                        </div>
                    </div>
                    <div class="col-sm-6">
                        <div class="box-body">
                            <div class="form-group">
                                <label class="col-sm-10 control-label text-left">Tax</label>
                                <div>
                                    <input min="0" autocomplete="off" value="0" class="text-right form-control billinfofo number " type="number" id="txtotherfee" placeholder="Other Tax" />
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="box-header with-border">
                    <h3 class="box-title">Total(s)</h3>
                </div>
                <div class="box-body text-right">
                    <div id="order_final_total">
                        <div class="form-group">
                            <label class="col-sm-10 control-label">Amount (excl. tax)</label>
                            <div class="col-sm-2 controls text-right">
                                <span id="SubTotal">0.00</span>
                            </div>
                        </div>
                        <div class="form-group ">
                            <label class="col-sm-10 control-label">Discount</label>
                            <div class="col-sm-2 controls text-right">
                                <span id="discountTotal">0.00</span>
                            </div>
                        </div>
                        <div style="display:none" class="form-group ">
                            <label class="col-sm-10 control-label">Avg. Cost</label>
                            <div class="col-sm-2 controls text-right">
                                <span id="avgcost">0.00</span>
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-sm-10 control-label">Shipping/Freight</label>
                            <div class="col-sm-2 controls text-right">
                                <span id="shippingTotal">0.00</span>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-10 control-label">Tax</label>
                            <div class="col-sm-2 controls text-right">
                                <span id="salesTaxTotal">0.00</span>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-10 control-label">Amount (inc. tax)</label>
                            <div class="col-sm-2 controls text-right">
                                <strong><span id="orderTotal">0.00</span></strong>
                            </div>
                        </div>
                        <div style="display:none" class="form-group">
                            <label class="col-sm-10 control-label">QtyTotal</label>
                            <div class="col-sm-2 controls text-right">
                                <strong>$<span id="QtyTotal">0.00</span></strong>
                            </div>
                        </div>
                        <div style="display:none" class="form-group">
                            <label class="col-sm-10 control-label">QtyRecTotal</label>
                            <div class="col-sm-2 controls text-right">
                                <strong>$<span id="QtyRecTotal">0.00</span></strong>
                            </div>
                        </div>
                    </div>
                </div>

                <!-- /.box-body -->
                <div class="box-footer text-right footer-finalbutton">
                    <button type="button" id="btnpoclosed" class="btn btn-danger" style="float:unset" data-toggle="tooltip" title="Close PO"><i class="far fa-btnpoclosed"></i>Close PO</button>
                    <a class="btn btn-danger pull-left" href="/Reception/ReceiveOrder">Back to List</a>
                </div>
            </div>
        </div>
    </div>
</section>
<style>
    .tbodycss {
        display: block;
        max-height: 300px;
        overflow-y: auto;
    }

        .theadcss, .tbodycss tr {
            display: table;
            width: 100%;
            table-layout: fixed;
        }

    .theadcss {
        width: calc( 100%)
    } 
</style>

<div class="modal in printable autoprint table-design not-fixed popuptable" id="POModal" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true"></div>
<script src="@Url.Content("~/js/Reception/ReceivePrint.js?v="+DateTime.Now.ToString("ddMMyyyymmss")+"")" asp-append-version="true"></script>

<script src="@Url.Content("~/js/Reception/NewReceive.js?v="+DateTime.Now.ToString("ddMMyyyymmss")+"")" asp-append-version="true"></script>

