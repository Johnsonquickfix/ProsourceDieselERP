                                            <th>Address</th>
                                            <th>SO No. Name</th>
                                            <th>Address</th>
                                            <th>Zip Code</th>
                                            <th>Planned date of delivery</th>
                                            <th>Status</th>
                                            <th>Action</th>
                                         
                                        </tr>
                                    </thead>
                                </table>
                            </div>
                        </div>
                    </div>

                </div>
            </div>
        </div>
    </div>
</section>
