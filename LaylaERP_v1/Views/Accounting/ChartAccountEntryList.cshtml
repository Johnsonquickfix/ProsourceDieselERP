
@{
    ViewBag.Title = "Chart_of_account_entry";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<link href="~/Content/AdminLTE-3.1.0/datatables/DataTables-1.10.20/css/dataTables.bootstrap.min.css" rel="stylesheet" />
<link href="~/Content/AdminLTE-3.1.0/datatables/Buttons-1.6.1/css/buttons.dataTables.min.css" rel="stylesheet" />
@*datatables js*@
<script src="~/Content/AdminLTE-3.1.0/datatables/datatables.min.js" type="text/javascript"></script>
<script src="~/Content/AdminLTE-3.1.0/datatables/DataTables-1.10.20/js/dataTables.bootstrap.min.js" type="text/javascript"></script>

<div class="page-title">
    <h2 class="page-heading">Chart of Account Entry</h2><a class="btn btn-danger" href="../../Accounting/ChartOfAccountEntry">
        <span title="Click here to add chart of account entry" data-placement="bottom" data-toggle="tooltip">Add Chart of Account Entry</span>
    </a>
</div>
<section class="content section-content journal-section table-design">
    <div class="row">
        <div class="col-md-12">
            <div class="box box-primary table-design not-fixed">
                <div class="box-body">
                    <table id="EmployeeListdata" class="inventory-table table-blue table check-table table-bordered table-striped dataTable">
                        <thead>
                            <tr>
                                <th>Id</th>
                                <th>Name</th>
                                <th>Type</th>
                                <th>Detail Type</th>
                                <th>Balance</th>
                                <th>Bank Balance</th>
                                <th>Date</th>
                                <th>Action</th>
                            </tr>
                        </thead>
                        <tfoot align="right">
                            <tr><th></th><th></th><th></th><th></th><th></th><th></th><th></th><th></th></tr>
                        </tfoot>
                    </table>

                </div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-md-5">
            <div class="form-group">
                <label for="grandtotal">Grand Total</label>
            </div>
        </div>
        <div class="col-md-2">
            <div class="form-group">
                <label id="txtbalance"></label>
            </div>
        </div>
        <div class="col-md-2">
            <div class="form-group">
                <label id="txtbankbalance"></label>
            </div>
        </div>
    </div>
</section>

<script src="@Url.Content("~/js/Accounting/ChartAccountEntryList.js?v="+DateTime.Now.ToString("ddMMyyyymmss")+"")" asp-append-version="true"></script>
