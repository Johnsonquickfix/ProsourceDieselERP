                                                    <span class="absolute-timing-hint" data-rh="This email will send on the same day as the trigger" data-rh-at="top"><i class="kl-icon kl-icon-trigger-bolt"></i>Day 0</span>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="placed-component-container exit_node">
                                        <div class="droppable-target droppable-node" draggable="true">
                                            <div class="component-node-container">
                                                <div class="add-component-node"><i class="fas fa-plus"></i></div>
                                            </div>
                                        </div>
                                        <div class="droppable-target droppable-node">
                                            <div class="component-node-container  ">
                                            </div>
