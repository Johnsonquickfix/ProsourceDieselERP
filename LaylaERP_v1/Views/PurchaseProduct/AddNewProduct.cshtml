<script type="text/javascript">
    //tinymce.init({
    //    selector: 'textarea#formatcustom',
    //    height: 200,
    //    plugins: 'table wordcount'
    //});

    $(document).ready(function () {
        $("#txtsrc").keyup(function (event) {
            if (event.keyCode === 13) {
                $("input[id*=btnsrc]").click();
            }
        });
        //    var urlperm = "/Product/ListProduct";
        //    CheckPermissions("#btnSave", "#hfEdit", "", urlperm);
        //    CheckPermissions("#btnedit", "#hfEdit", "", urlperm);
        //    CheckPermissions("#btnSaveupdate", "#hfEdit", "", urlperm);
    });
    $("input[id*=btnsrc]").click(function () {
        var searchvalue = $("input[id*=txtsrc]").val();
        if (searchvalue != "") {
            $('.tblprodctype .categorychecklist').each(function () {
                $(this).find('li').each(function () {
                    var current = $(this);
                    var s = current.text().trim().toLowerCase();
                    //console.log(s);
                    //console.log(searchvalue);
                    if (s.match(searchvalue.toLowerCase())) {
                        $(this).show();
                    }
                    else {
                        $(this).hide();
                        $(this.nextSibling).wrap('<span style="display:none"></style>');
                    }

                })
            })
        }
        else {
            $("#cboAll,.w3-check").show();
        }

    })
</script>

