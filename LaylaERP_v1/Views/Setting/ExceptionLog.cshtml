
@{
    ViewBag.Title = "ExceptionLog";
}
<form method="post">
    <div class="col-md-4">
        <div class="form-group">
            <label for="LogDate">Date of Birth <span style="color:red;">*</span></label>
            <div class="input-group">
                <input type="text" class="form-control" name="txtLogDate" id="txtLogDate" /><span class="even-space"></span>
            </div>
        </div>
    </div>
    <div class="box-footer text-right">
        <button type="submit" id="btnChangeDate" class="btn btn-danger">Change</button>
    </div>
</form>
    @foreach (string text in ViewBag.Data)
    {
        <h4>@text</h4>
    }

    <script>
        $(document).ready(function () {
            $("#txtLogDate").datepicker({ format: 'mm-dd-yyyy', });
        })
    </script>
