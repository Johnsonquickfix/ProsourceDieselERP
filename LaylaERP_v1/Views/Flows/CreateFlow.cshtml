            <div class="col-12">
                <div class="page-title-box d-sm-flex align-items-center justify-content-between m-2">
                    <div>
                        <ol class="breadcrumb m-0 font-size-18">
                            <li class="breadcrumb-item"><a href="/flows/index">Flows</a></li>
                            <li name="flow-title" class="breadcrumb-item"></li>
                        </ol>
                    </div>
                    <div class="page-title-right">
                    </div>
                </div>
            </div>
        </div>
        <!-- end page title -->
        <div class="flows-main">
            <aside id="leftcard" class="configuration-panel">
                <div class="configuration-panel-header">
                    <div class="configuration-panel-title">
