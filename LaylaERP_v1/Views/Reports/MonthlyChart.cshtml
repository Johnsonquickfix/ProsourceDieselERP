
@{
    ViewBag.Title = "MonthlyChart";
}

<script src="~/Content/highcharts/highcharts.js"></script>
<script src="~/Content/highcharts/series-label.js"></script>
<script src="~/Content/highcharts/data.js"></script>
<script src="~/Content/highcharts/exporting.js"></script>
<script src="~/Content/highcharts/export-data.js"></script>
<script src="~/Content/highcharts/accessibility.js"></script>

<div id="loader" class="d-flex align-items-center">
    <img src="data:image/gif;base64," align="">
    <span>&nbsp;&nbsp;Loading......</span>
</div>
<div class="page-title">
    <h2> Monthly Sales Order Chart</h2>
</div>
<figure class="highcharts-figure">
    <div class="row">
        <input type="hidden" id="hfType" value="" />
        <div class="col-md-12">
            <div class="user-top-section">
                <div class="tablenav top tablenav-top2">
                    <div class="alignleft actions">
                        <div class="input-group group_inpt">
                            <label class="control-label">Year<span style="color:red;"> *</span></label>
                            @*<select class="form-control" id="year">
            <option value="">Year</option>
            <option value="2019">2019</option>
            <option value="2020">2020</option>
            <option selected value="2021">2021</option>
            <option value="2022">2022</option>
        </select>*@
                        <select class="form-control" id="year">
                            <option value="">Year</option>
                        </select>
                        </div>
                    </div>
                    <div class="alignleft actions">
                        @*<input type="submit" name="filter_action" id="btnOtherFilter" class="button" value="Filter" data-toggle="tooltip" title="Search order by selection criteria.">*@
                        <button type="button" id="btnsearch" onclick="Search();" class="btn btn-primary btn-flat button">Filter</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div id="container"></div>

</figure>
<script type="text/javascript">

        $(document).ready(function () {
            $("#loader").hide();
            globalyear('year');
            $(function () {
                Highcharts.setOptions({
                    lang: {
                        thousandsSep: ','
                    }
                });
            })
            //var currentYear = new Date().getFullYear();
            //var yearSelect = document.getElementById('year');
            //for (var i = -2; i < 5; i++) {
            //    var isSelected = currentYear === currentYear - i
            //    yearSelect.options[yearSelect.options.length] = new Option(currentYear - i, currentYear - i, isSelected, isSelected);
            //}
            Search();
        });
        function Search() {
            $("#loader").show();
            let Year = $("#year").val();;
            let ed = "0";
            var account = "1";
            $.ajax({
                type: 'POST',
                url: '/Reports/GetMonthly',
                data: { Month: Year, Year: ed, Type: account },
                dataType: 'json',
                beforeSend: function () {
                    $("#loader").show();
                },
                success: function (result) {

                    var data = JSON.parse(result);
                    console.log(data.val);
                   // console.log(data.result);
                 
                    if (data.length > 0) {
                        _list = [                             
                            { name: 'January', drilldown: 'January', y: data[0].January },
                            { name: 'February', drilldown: 'February', y: data[0].February },
                            { name: 'March', drilldown: 'March', y: data[0].March },
                            { name: 'April', drilldown: 'April', y: data[0].April },
                            { name: 'May', drilldown: 'May', y: data[0].May },
                            { name: 'June', drilldown: 'June', y: data[0].June },
                            { name: 'July', drilldown: 'July', y: data[0].July },
                            { name: 'August', drilldown: 'August', y: data[0].August },
                            { name: 'September', drilldown: 'September', y: data[0].September },
                            { name: 'October', drilldown: 'October', y: data[0].October },
                            { name: 'November', drilldown: 'November', y: data[0].November },
                            { name: 'December', drilldown: 'December', y: data[0].December },
                        ];
                      bindChart();
                   }
                    else {
                        _list = [
                            { name: 'January', drilldown: 'January', y: 0 },
                            { name: 'February', drilldown: 'February', y: 0 },
                            { name: 'March', drilldown: 'March', y: 0 },
                            { name: 'April', drilldown: 'April', y: 0 },
                            { name: 'May', drilldown: 'May', y: 0 },
                            { name: 'June', drilldown: 'June', y: 0 },
                            { name: 'July', drilldown: 'July', y: 0 },
                            { name: 'August', drilldown: 'August', y: 0 },
                            { name: 'September', drilldown: 'September', y: 0 },
                            { name: 'October', drilldown: 'October', y: 0 },
                            { name: 'November', drilldown: 'November', y: 0 },
                            { name: 'December', drilldown: 'December', y: 0 },
                        ];

                        bindChart();
                    }
                },
                error: function (Result) { alert("Error"); },
                complete: function () { $("#loader").hide(); }
            });
        }
        function bindChart() {
            // Create the chart

            console.log(_list);
            Highcharts.chart('container', {
                chart: {
                    type: 'column'
                },
                title: {
                    text: 'Year wise monthly sales order chart'
                },
                //subtitle: {
                //    text: 'Click the columns to view versions. Source: <a href="http://statcounter.com" target="_blank">statcounter.com</a>'
                //},
                accessibility: {
                    announceNewData: {
                        enabled: true
                    }
                },
                xAxis: {
                    type: 'category'
                },
                yAxis: {
                    title: {
                        text: 'Amount'
                    }

                },
                legend: {
                    enabled: false
                },
                plotOptions: {
                    series: {
                        borderWidth: 0,
                        dataLabels: {
                            enabled: true,
                            format: '{point.y:,.2f}'
                        }
                    }
                },

                tooltip: {
                    pointFormat: '<span style="color:{point.color}">{point.name}</span>: <b>{point.y:,.2f}</b><br/>'
                },

                series:
                    [
                        {

                            colorByPoint: true,
                            data: _list
                            //        data: [
                            //            {
                            //                name: "Chrome",
                            //                y: 62.74,
                            //                drilldown: "Chrome"
                            //            },
                            //            {
                            //                name: "Firefox",
                            //                y: 10.57,
                            //                drilldown: "Firefox"
                            //            },
                            //            {
                            //                name: "Internet Explorer",
                            //                y: 7.23,
                            //                drilldown: "Internet Explorer"
                            //            },
                            //            {
                            //                name: "Safari",
                            //                y: 5.58,
                            //                drilldown: "Safari"
                            //            },

                            //        ]
                        }
                    ]
                ,

            });
            $('.highcharts-credits').remove();
    }


</script>

<script src="~/js/globalyear.js"></script>