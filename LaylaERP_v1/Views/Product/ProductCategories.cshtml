<input type="hidden" id="tocolom"/>
<input type="hidden" id="fromcolom"/>
<input type="hidden" id="hfEdit" value="" />
<div id="loader" class="d-flex align-items-center">
    <img src="data:image/gif;base64," align="">
    <span>&nbsp;&nbsp;Loading......</span>
</div>
<div class="page-title">
    <h2 class="page-heading" id="lbltitle">Categories</h2>
    <!--<a id="btnbackproduct" title="Click here to back list" data-toggle="tooltip" data-placement="bottom" class="backtoback btn btn-danger">Back to Product</a>-->
    <button class="btn btn-danger" id="btnAddCategory" title="Add new category" data-toggle="tooltip" data-placement="bottom">Add category</button>
</div>
<section class="content section-content category-section">
    <div class="row rowbox" id="ProdCat">
        <input type="hidden" id="hfid" value="" />
        <input type="hidden" id="hfMetaid" value="" />
        <div class="col-md-4" id="ProdCatAdd">
            <div class="box box-primary">
                <div class="box-header with-border">
                    Product categories for your store can be managed here.
                </div>
                <div class="box-header with-border">
                    <h3 class="box-title" id="lblNewCategory"><b>Add new category</b></h3>
                </div>
                <div class="box-body">
                    <div class="form-group">
                        <label class="control-label">Name</label>
                        <input type="text" id="txtCategoryName" class="form-control" />
                        <span class="form-info">The name is how it appears on your site.</span>
                    </div>
                    <div class="form-group">
                        <label class="control-label">Slug</label>
                        <input type="text" id="txtCategorySlug" class="form-control" />
                        <span class="form-info">The “slug” is the URL-friendly version of the name. It is usually all lowercase and contains only letters, numbers, and hyphens.</span>
                    </div>
                    <div class="form-group">
                        <label class="control-label">Parent category</label>
                        @*<select class="form-control select2" id="ddlParentCategory"></select>*@
                        <Select class="form-control" id="ddlParentCategory" style="width: 100%;">
                            <option></option>
                        </Select>
                        <span class="form-info">
                            Assign a parent term to create a hierarchy.
                        </span>
                    </div>
                    <div class="form-group">
                        <label class="control-label">Description</label>
                        <textarea class="form-control" rows="8" id="txtDescription"></textarea>
                        <span class="form-info">The description is not prominent by default.</span>
                    </div>
                    <div class="form-group">
                        <label class="control-label">Display type</label>
                        <select class="form-control" id="ddlDisplayType">
                            <option value="products">Products</option>
                            <option value="default">Default</option>

                            @*<option value="subcategories">Subcategories</option>*@
                            @*<option value="both">Both</option>*@
                        </select>
                    </div>
                    <div class="form-group">
                        <!--Fiture Thumbnail-->
                        <label class="control-label">Feature Thumbnail</label>
                        @* <input type="file" name="ImageFile" id="ImageFile" data-placement="bottom" title="choose a file" data-toggle="tooltip" style="" value="" />*@
                        <div class="image-upload-box d-flex">
                            <img src="~/Images/varient_pic.png" id="getImageFileThumbnail" />
                            <div class="upload-btn-wrapper">
                                <!-- Trigger the modal with a button -->
                                <button type="button" class="btn-upload btn btn-danger" data-toggle="modal" data-target="#myFitureThumbnail">Upload/Add Image</button>

                                @*<button class="btn-upload btn btn-danger">Upload/Add Image</button>*@
                                <!--<input type="file" name="ImageFile" id="ImageFile" />-->
                                @*<input id="fuAddImage" type="file" name="myfile" />*@
                            </div>
                          
                        </div>

                        <!-- Modal -->
                        <div class="modal fade" id="myFitureThumbnail" role="dialog">
                            <div class="modal-dialog modal-lg">
                                <div class="modal-content">
                                    <div class="modal-header">
                                        <button type="button" class="close" data-dismiss="modal">&times;</button>
                                        <h4 class="modal-title">Choose an Feature Thumbnail image</h4>
                                        <button id="UFile" onclick="UploadFile()">Uplode File</button>
                                        <button id="" onclick="MediaLibrary(0);">Media Library</button>
                                    </div>
                                    <div class="modal-body">
                                        <div class="container Upload" id="" style="display:none">
                                            <div class="" id="">
                                                <h3>Choose files to upload</h3>
                                              
                                                <input type="file" id="FileUploadThumbnail" multiple />
                                            </div>
                                        </div>
                                        <div class="container MLibrary" id="" style="display: none;">
                                            <div class="row">
                                                <div class="col-sm-2 ">
                                                    <p>Feature media</p>
                                                    <select class="form-control">
                                                        <option value="0">All Dates</option>
                                                        <option value="0"></option>
                                                        <option value="0">Month</option>
                                                        <option value="0">Month</option>
                                                        <option value="0">Month</option>
                                                        <option value="0">Month</option>
                                                        <option value="0">Month</option>
                                                        <option value="0">Month</option>
                                                        <option value="0">Month</option>
                                                        <option value="0">Month</option>
                                                        <option value="0">Month</option>
                                                        <option value="0">Month</option>
                                                    </select>
                                                </div>
                                                <div class="col-sm-5"></div>
                                                <div class="col-sm-2">
                                                    <p>Search</p>
                                                    <input type="text" class="form-control" id="SearchC" />
                                                </div>
                                            </div>
                                            <div style="width: 800px; height: 300px; overflow-x: hidden; overflow-y: auto; text-align: center;">
                                                <div>
                                                    <ul class="MLibraryThumb" id="">
                                                    </ul>
                                                </div>
                                                <br />
                                                <div>
                                                    <span class="totalData"></span>
                                                    <br />
                                                    <button type="button" class="btn btn-success center" onclick="MediaLibrary()">Load More</button>
                                                </div>
                                            </div>
                                            <div>

                                            </div>
                                        </div>
                                    </div>
                                    <div class="modal-footer">
                                        <button type="button" id="useImgThumb" class="btn btn-primary" data-dismiss="modal" onclick="UseImagemyFitureThumbnail();">Use Image</button>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <!-- Banner-->
                        <label class="control-label">Banner</label>
                        @* <input type="file" name="ImageFile" id="ImageFile" data-placement="bottom" title="choose a file" data-toggle="tooltip" style="" value="" />*@
                        <div class="image-upload-box d-flex">
                            <img src="~/Images/varient_pic.png" id="getImageFileBanner" />
                            <div class="upload-btn-wrapper">
                                <!-- Trigger the modal with a button -->
                                <button type="button" class="btn-upload btn btn-danger" data-toggle="modal" data-target="#myBanner">Upload/Add Image</button>

                                @*<button class="btn-upload btn btn-danger">Upload/Add Image</button>*@
                                <!--<input type="file" name="ImageFile" id="ImageFile" />-->
                                @*<input id="fuAddImage" type="file" name="myfile" />*@
                            </div>
                           
                        </div>

                        <!-- Modal -->
                        <div class="modal fade" id="myBanner" role="dialog">
                            <div class="modal-dialog modal-lg">
                                <div class="modal-content">
                                    <div class="modal-header">
                                        <button type="button" class="close" data-dismiss="modal">&times;</button>
                                        <h4 class="modal-title">Choose an Banner image</h4>
                                        <button id="UFile" onclick="UploadFile()">Uplode File</button>
                                        <button id="" onclick="MediaLibrary(0);">Media Library</button>
                                    </div>
                                    <div class="modal-body">
                                        <div class="container Upload" id="" style="display:none">
                                            <div class="" id="">
                                                <h3>Choose files to upload</h3>
                                                <p>Or</p>
                                                <input type="file" id="FileUploadBanner" multiple />
                                            </div>
                                        </div>
                                        <div class="container MLibrary" id="" style="display: none;">
                                            <div class="row">
                                                <div class="col-sm-2 ">
                                                    <p>Filter media</p>
                                                    <select class="form-control">
                                                        <option value="0">All Dates</option>
                                                        <option value="0"></option>
                                                        <option value="0">Month</option>
                                                        <option value="0">Month</option>
                                                        <option value="0">Month</option>
                                                        <option value="0">Month</option>
                                                        <option value="0">Month</option>
                                                        <option value="0">Month</option>
                                                        <option value="0">Month</option>
                                                        <option value="0">Month</option>
                                                        <option value="0">Month</option>
                                                        <option value="0">Month</option>
                                                    </select>
                                                </div>
                                                <div class="col-sm-5"></div>
                                                <div class="col-sm-2">
                                                    <p>Search</p>
                                                    <input type="text" class="form-control" id="SearchC" />
                                                </div>
                                            </div>
                                            <div style="width: 800px; height: 300px; overflow-x: hidden; overflow-y: auto; text-align: center;">
                                                <div>
                                                    <ul class="MLibraryThumb" id="">
                                                    </ul>
                                                </div>
                                                <br />
                                                <div>
                                                    <span id="totalData"></span>
                                                    <br />
                                                    <button type="button" class="btn btn-success center" onclick="MediaLibrary()">Load More</button>
                                                </div>
                                            </div>
                                            <div>

                                            </div>
                                        </div>
                                    </div>
                                    <div class="modal-footer">
                                        <button type="button" id="useImgBanner" class="btn btn-primary" data-dismiss="modal" onclick="UseImagemyBanner();">Use Image</button>
                                    </div>
                                </div>
                            </div>
                        </div>



                    </div>
                </div>
                <div class="box-footer text-right">
                    <button class="btn btn-danger" id="btnReset" data-placement="bottom" title="Reset all" data-toggle="tooltip">Reset</button>
                    <button class="btn btn-danger" id="btnAddNewCategory" data-toggle="tooltip">Create</button>
                </div>
            </div>
        </div>
        <div class="col-md-8">
            <div class="box box-primary">
                <div class="box-header with-border">
                    <div class="user-top-section">
                        <div class="tablenav top tablenav-top2">
                            <div class="alignleft actions bulkactions">
                                <select class="form-control" id="ddlStatus">
                                    <option value="1">Bulk Action</option>
                                    <option value="2">Delete</option>
                                </select>
                                <button type="button" id="btnApply" data-placement="bottom" title="apply bulk action" data-toggle="tooltip" class="btn btn-primary btn-flat button">Apply</button>
                            </div>
                            <div class="alignright actions bulkactions">
                                <input type="text" id="txtSearchCategory" class="form-control" style="display:none;" />
                                <button type="button" id="btnSearchCategory" title="Search categories" data-toggle="tooltip" class="btn btn-primary btn-flat button" style="display:none;">Search categories</button>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="box-body">
                    <table id="ProductCategory" class="table-blue table check-table table-bordered">
                        <thead>
                            <tr role="row">
                                <th>@*<input type="checkbox" name="checkAll" id="checkAll"><label></label>*@</th>
                                <th>Image</th>
                                <th>Name</th>
                                <th>Description</th>
                                <th>Slug</th>
                                <th>Count</th>
                                <th>Action</th>
                            </tr>
                        </thead>
                    </table>
                </div>
            </div>
        </div>
    </div>
</section>
<script src="@Url.Content("~/js/Product/ProductCategory.js?v="+DateTime.Now.ToString("ddMMyyyymmss")+"")" asp-append-version="true"></script>
<script src="@Url.Content("~/js/Users/RolePermission.js?v="+DateTime.Now.ToString("ddMMyyyymmss")+"")" asp-append-version="true"></script>