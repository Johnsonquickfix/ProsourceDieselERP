
@{
    ViewBag.Title = "AddAccount";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<link href="~/Content/css/Style.css" rel="stylesheet" />
<div id="loader" class="d-flex align-items-center">
    <img src="data:image/gif;base64," align="">
    <span>&nbsp;&nbsp;Loading......</span>
</div>
<div class="page-title">
    <h2 class="page-heading">Add Account</h2><span title="Click here to back to list of chart of accoounts" data-placement="bottom" data-toggle="tooltip">@Html.ActionLink("Back to List", "chartofaccounts", null, new { @class = "btn btn-danger" })</span>
</div>
        <section class="content content-section mt-1">
            <div class="createuser-section section_wrapper">
                <div class="create-user-box">
                    <!-- /.col -->
                    <div class="row" style="background-color:white">
                        <div class="box box-primary">
                            <div class="box-body">
                                <div class="col-sm-6">
                                    <div class="form-group">
                                        <div title="Enter Ref">
                                            <label for="exampleInputEmail1">Chart Of Accounts<span style="color:red">*</span></label>
                                            <input type="text" class="form-control" id="txtChartOfAccounts" placeholder="Chart of Account " value="US-BASE">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <div title="Enter Short name location">
                                            <label for="exampleInputEmail1">Account Number<span style="color:red">*</span></label>
                                            <input type="email" class="form-control" id="txtAccountNumber" placeholder="Enter Account Number">
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <div title="Enter Description">
                                            <label for="exampleInputEmail1">Name<span style="color:red">*</span></label>
                                            <input type="text" class="form-control" id="txtLabel" placeholder="Enter Name">
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <div title="Enter E-mail">
                                            <label for="exampleInputEmail1">Type<span style="color:red">*</span></label>
                                            <select class="form-control" id="ddlParentAccount">
                                                <option value="0">Please select type</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <div title="Enter E-mail">
                                            <label for="exampleInputEmail1">Detail Type<span style="color:red"></span></label>
                                            <input type="text" class="form-control" id="txtdetailtype" placeholder="Enter Detail Type">
                                        </div>
                                    </div>
                                </div>
                                <div class="col-sm-6">
                                    <div class="form-group">
                                        <div title="Enter E-mail">
                                            <label for="exampleInputEmail1">Chart Tag<span style="color:red"></span></label>
                                            <select class="form-control" id="ddltag">
                                                <option value="">Please select chart tag</option>
                                                <option value="E">Expense</option>
                                                <option value="I">Income</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <div title="Enter E-mail">
                                            <label for="exampleInputEmail1">Account Tag<span style="color:red"></span></label>
                                            <select class="form-control" id="ddlactype">
                                                <option value="">Please select account type</option>
                                                <option value="BS">Balance sheet</option>
                                                <option value="PL">Profit and loss</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <div title="Enter E-mail">
                                            <label for="exampleInputEmail1">Balance sheet type<span style="color:red"></span></label>
                                            <select class="form-control" id="ddlbstype">
                                                <option value="">Please select balance sheet type</option>
                                                <option value="AS">Assets</option>
                                                <option value="LB">Liabilities</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <div title="Enter E-mail">
                                            <label for="exampleInputEmail1">Account category<span style="color:red"></span></label>
                                            <select class="form-control" id="ddlaccountcategory"></select>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <div title="Enter E-mail">
                                            <label for="name">Sub-Account</label>
                                            <select class="form-control select2" id="ddlSubAccount"></select>
                                        </div>
                                    </div>
                                    <div class="box-footer">
                                        <div class="text-right">
                                            <a href="#" class="btn btn-danger" id="btnSave"><span title="Save details of chart of accounts" data-placement="left" data-toggle="tooltip"><b>Submit</b></span></a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                     </div>
                </div>
             </div>
        </section>

        <script>
            $(document).ready(function () {
                $("#loader").hide();
                $(".select2").select2();
                getAccountCategory();
                $(document).on('click', "#btnSave", function () {
                    AddAccount();
                    
                });

                $.get('/Accounting/GetParentAccount', function (data) {
                    var items = "";
                    $.each(data, function (index, value) {
                        items += $('<option>').val(this['Value']).text(this['Text']).appendTo("#ddlParentAccount");

                    })
                });

                isEdit(true);
                getsubaccount();
            });

            function getsubaccount() {
                $.ajax({
                    url: "/Accounting/GetName",
                    type: "Get",
                    success: function (data) {
                        $('#ddlSubAccount').append('<option value="0">Please select sub account</option>');
                        for (var i = 0; i < data.length; i++) {
                            $('#ddlSubAccount').append('<option value="' + data[i].Value + '">' + data[i].Text + '</option>');
                        }
                    }, async: false
                });
            }

            function isEdit(val) {
                localStorage.setItem('isEdit', val ? 'yes' : 'no');
            }

            function AddAccount() {
                debugger
                var groupofaccount1 = $("#ddlParentAccount option:selected");
                var groupofaccount = groupofaccount1.text();
                //alert(groupofaccount);
                pcgversion = $("#txtChartOfAccounts").val();
                accountnumber = $("#txtAccountNumber").val();
                label = $("#txtLabel").val();
                parentaccount = $("#ddlParentAccount").val();
                //groupofaccount = $("#selector option:selected");
                var detailtype = $("#txtdetailtype").val();
                var tag = $("#ddltag").val();
                var actype = $("#ddlactype").val();
                var bstype = $("#ddlbstype").val();
                var accountcategory = $("#ddlaccountcategory").val();
                var subaccount = $("#ddlSubAccount").val();
                if (pcgversion == "") {
                    swal('Alert', 'Please enter chart of account', 'error').then(function () { swal.close(); $('#txtChartOfAccounts').focus(); });
                }
                else if (accountnumber == "") {
                    swal('Alert', 'Please enter account number', 'error').then(function () { swal.close(); $('#txtAccountNumber').focus(); });
                }
                else if (label == "") {
                    swal('Alert', 'Please enter name', 'error').then(function () { swal.close(); $('#txtLabel').focus(); });
                }
                else if (parentaccount == 0) {
                    swal('Alert', 'Please select type', 'error').then(function () { swal.close(); $('#ddlParentAccount').focus(); });
                }
                //else if (groupofaccount == "") {
                //    swal('Alert', 'Please Enter Group Of Account', 'error').then(function () { swal.close(); $('#txtGroupOfAccount').focus(); });
                //}

                else {

                    var obj = {
                        fk_pcg_version: pcgversion,
                        account_number: accountnumber,
                        account_parent: parentaccount,
                        label: label,
                        pcg_type: groupofaccount,
                        labelshort: detailtype,
                        extraparams: tag,
                        ac_type: actype,
                        bs_type: bstype,
                        fk_accounting_category: accountcategory,
                        Sub_Account: subaccount,
                    }
                    $.ajax({
                        url: '/Accounting/AddAccount/', dataType: 'json', type: 'Post',
                        contentType: "application/json; charset=utf-8",
                        data: JSON.stringify(obj),
                        dataType: "json",
                        beforeSend: function () { $("#loader").show(); },
                        success: function (data) {
                            if (data.status == true) {
                                swal('Success', data.message, 'success').then((result) => { location.href = '../../Accounting/chartofaccounts'; });
                                reset();
                            }
                            else {
                                swal('Alert!', data.message, 'error');
                            }
                        },
                        complete: function () { $("#loader").hide(); isEdit(false); },
                        error: function (error) { swal('Error!', 'something went wrong', 'error'); },
                    })

                }

            }

            function reset() {
                $("#txtAccountNumber").val('');
                $("#txtLabel").val('');
                $("#ddlParentAccount").val('0');
                $("#txtdetailtype").val('');
            }
    

</script>
<script>
    function getAccountCategory() {
        $.ajax({
            url: "/Accounting/GetAccountCategory",
            type: "Get",
            success: function (data) {
                $('#ddlaccountcategory').append('<option value="0">Please select account category</option>');
                for (var i = 0; i < data.length; i++) {
                    $('#ddlaccountcategory').append('<option value="' + data[i].Value + '">' + data[i].Text + '</option>');
                }
            }, async: false
        });
    }
</script>