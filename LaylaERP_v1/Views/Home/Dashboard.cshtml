<script type="text/javascript">

    google.load("visualization", "1", { packages: ["corechart"] });
    //google.charts.load("current", { packages: ["corechart"] });
    google.setOnLoadCallback(drawChart);
    function drawChart(fromdate, todate) {
        //var fromdate = $('#DateRange2').data('daterangepicker').startDate.format('MM-DD-YYYY');
        //var todate = $('#DateRange2').data('daterangepicker').endDate.format('MM-DD-YYYY');
        var obj = { strValue5: fromdate, strValue6: todate };
        $.ajax({
            type: "POST",
            url: "/Home/SalesGraph/",
            data: JSON.stringify(obj),
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (r) {
                var data = google.visualization.arrayToDataTable(r);
                var options = {
                    title: 'Sales',
                    is3D: true,
                };
                var chart = new google.visualization.LineChart($("#saleschart")[0]);
                chart.draw(data, options);
            },
            failure: function (r) {
                alert(r.d);
            },
            error: function (r) {
                alert(r.d);
            }
        });
    }
</script>

