
@{
    ViewBag.Title = "ExceptionLog";
}
<div class="col-md-4">
    <div class="form-group">
        <label for="LogDate">Date of Birth <span style="color:red;">*</span></label>
        <div class="input-group">
            <input type="text" class="form-control" id="txtLogDate" /><span class="even-space"></span>
        </div>
    </div>
</div>
@foreach (string text in ViewBag.Data)
{
    <h4>@text</h4>
}

<script>
    $(document).ready(function () {
        $("#txtLogDate").datepicker({ format: 'mm-dd-yyyy', });
    })
</script>