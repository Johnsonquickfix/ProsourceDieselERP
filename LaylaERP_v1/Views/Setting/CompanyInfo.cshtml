@model dynamic
@{
    ViewBag.Title = "CompanyDetails";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<script src="~/Scripts/jquery.js" type="text/javascript"></script>
<script src="~/Scripts/passwordStrengthMeter.js" type="text/javascript"></script>
<link href="~/Content/css/Style.css" rel="stylesheet" />
<div id="loader" class="d-flex align-items-center">
    <img src="data:image/gif;base64," align="">
    <span>&nbsp;&nbsp;Loading......</span>
</div>
<div class="page-title">
    <h2 class="page-heading">Company Details</h2>
    <a class="btn btn-danger" href="Users"><span title="Click here to back to user's list" data-placement="bottom" data-toggle="tooltip">Back to List</span></a>
</div>
<div class="createuser-section section_wrapper">
    <div class="create-user-box">
        <div class="row">
            <div class="col-md-5">

                <!-- Profile Image -->
                <div class="box box-primary">
                    <span class="edit-pic-profile">
                        <input type="file" onchange="readURL(this);" name="file" id="file" class="inputfile" />
                        <label id="btnPhotoLibrary" for="file"><i class="fa fa-edit"></i>Edit</label>
                    </span>
                    @if (Model.User_Image.Length > 0 && Model.User_Image != null)
                    {
                        <img runat="server" id="show_picture" src="data:image;base64,@System.Convert.ToBase64String(Model.User_Image)">
                    }
                    else
                    {
                        <img runat="server" id="show_picture" src="~/Images/userprofile2.png" alt="User profile picture">
                     }
                    <input runat="server" type="hidden" id="hidimage" /><input type="hidden" id="hidimagetype" />
                                @*<canvas style="visibility: hidden" width="100%" height="100%" id="canvas"></canvas>

                    <div class="box-body box-profile">
                        <div class="user-box-profile">
                            <div class="add-profile">


                            </div>
                        </div>

                    </div>*@

                    @*<h3 class="profile-username text-center">Layla Sleep</h3>
        <p class="text-muted text-center">laylasleep@acb.com</p>
        <p class="text-center">Washington Dc</p>*@
                </div>
                <!-- /.box-body -->
            </div>

            <!-- /.col -->
            <div class="col-md-7">
                <div class="box box-primary create-user-primary">
                    <div class="box-header with-border">
                        <div class="col-sm-4"> 
                            <div class="form-group">
                                <input type="hidden" id="hfid" value="@Model.ID" />
                                <input type="hidden" id="hfzipstatus" /> 
                            </div>
                            <div class="form-group">
                                <label for="exampleInputEmail1">E-mail<span style="color:red;"> *</span></label>
                                <input type="text" class="form-control" value="@Model.email" id="txtEmail" placeholder="Enter Your Mail Address">
                            </div>
                            <div class="form-group">
                                <label for="exampleInputEmail1">Name<span style="color:red;"> *</span></label>
                                <input type="text" class="form-control" value="@Model.name" id="txtName" placeholder="First Name">
                            </div>
                            
                            <div class="form-group">
                                @{
                                    var selected = "";
                                    var selected1 = "";
                                    if (Model.user_status == 0)
                                    {
                                        selected = "selected=selected";
                                    }
                                    else
                                    {
                                        selected = "";
                                    }
                                    if (Model.user_status == 1)
                                    {
                                        selected1 = "selected=selected";
                                    }
                                    else
                                    {
                                        selected1 = "";
                                    }
                                }
                                <label for="exampleInputEmail1">User Status<span style="color:red;"> *</span></label>
                                <select class="form-control" id="ddlstatus">
                                    <option @selected value="0">Active</option>
                                    <option @selected1 value="1">InActive</option>
                                </select>
                            </div>
                        </div>
                        <div class="col-sm-4">

                            <div class="form-group">
                                <label for="exampleInputEmail1">Display Name<span style="color:red;"> *</span></label>
                                <input type="text" class="form-control" value="@Model.display_name" id="txtdiplayname" placeholder="Display Name">
                            </div>
                            <div class="form-group">
                                <label for="exampleInputEmail1">Contact Number<span style="color:red;"> *</span></label>
                                <input type="text" class="form-control" id="txtPhone" value="@Model.phone" placeholder="Enter Your Phone Number">
                            </div>

                        </div>
                         
                    </div>
                    <div class="box-footer text-right">
                        <a href="javascript:void(0);" class="btn btn-danger" id="btnSave"><span title="Click here to update user's details" data-placement="left" data-toggle="tooltip"><b>Update User</b></span></a>
                    </div>
                </div>
                <!-- /.nav-tabs-custom -->
            </div>
            <!-- /.col -->
        </div>
    </div>
</div>

<script>
    $(document).ready(function () {
        $("#loader").hide();  
        $(document).on('click', "#btnSave", function () {
            Addcompany();
        }) 
        isEdit(true); 
    });
     
    function Addcompany() {
       
        //var ImageURL;
        //var canvas = document.createElement('canvas');
        //var objimg = document.getElementById('show_picture');
        //canvas.width = objimg.width;
        //canvas.height = objimg.height;
        //var ctx = canvas.getContext('2d').drawImage(objimg, 0, 0, canvas.width, canvas.height);
        //var can = document.getElementById('canvas');
        //ImageType = "jpeg";
        //var img = canvas.toDataURL("image/jpeg", 30 / 100);
        //var result_image_obj = new Image();
        //result_image_obj.src = img;
        //$("#show_picture").attr("src", result_image_obj.src);
        //ImageURL = result_image_obj.src;
        //var block = ImageURL.split(";");
        //var realData = block[1].split(",")[1];

        var imgElement = document.getElementById('show_picture');
        var imageSource = imgElement.src;
        var block = imageSource.split(";");
        var realData = block[1].split(",")[1]; 
        //$("#hidimage").val(realData); 
        Picture = realData; // $("#hidimage").val();
        ID = $("#hfid").val();
        Email = $("#txtEmail").val();
        Name = $("#txtName").val();
        displayname = $("#txtdiplayname").val(); 
        Phone = $("#txtPhone").val(); 
        user_status = $("#ddlstatus").val();
        


        if (displayname == "") {
            swal('Alert', 'Please enter display name', 'error').then(function () { swal.close(); $('#txtdiplayname').focus(); });
        }
        else if (Email == "") {
            swal('Alert', 'Please enter email', 'error').then(function () { swal.close(); $('#txtEmail').focus(); });
        }
        else if (Name == "") {
            swal('Alert', 'Please enter  name', 'error').then(function () { swal.close(); $('#txtName').focus(); });
        }
        else if (Phone == "") {
            swal('Alert', 'Please enter phone', 'error').then(function () { swal.close(); $('#txtPhone').focus(); });
        }
            else { 
                    var obj = {
                        ID: 3,
                        user_email: Email,
                        user_nicename: displayname,
                        first_name: Name,
                        phone: Phone, 
                        user_status: user_status,
                        User_Image: Picture,
                    }
                    $.ajax({
                        url: '/Setting/UpdateCompany/', dataType: 'json', type: 'Post',
                        contentType: "application/json; charset=utf-8",
                        data: JSON.stringify(obj),
                        dataType: "json",
                        beforeSend: function () { $("#loader").show(); },
                        success: function (data) {
                            if (data.status == true) {
                                swal('Success!', data.message, 'success');
                                    //.then((result) => { location.href = 'Users'; });
                            }
                            else {
                                swal('Alert!', data.message, 'error');
                            }
                        },
                        complete: function () { $("#loader").hide(); isEdit(false); },
                        error: function (error) { swal('Error!', 'something went wrong', 'error'); },
                    }) 
            }
        }
   
    function isEdit(val) {
        localStorage.setItem('isEdit', val ? 'yes' : 'no');
    }
    function readURL(input) {
        if (input.files && input.files[0]) {
            var reader = new FileReader();
            reader.onload = function (e) {
                $('#show_picture').attr('src', e.target.result);
            };
            reader.readAsDataURL(input.files[0]);
        }
    }
</script>
 
 