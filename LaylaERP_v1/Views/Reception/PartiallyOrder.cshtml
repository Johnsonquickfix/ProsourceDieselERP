
@{
    ViewBag.Title = "Received Orders";

}

<div class="page-title">
    <h2 class="page-heading">Received Orders List</h2>
    <div class="user-top-section">
        <div class="tablenav top tablenav-top2">
            <div class="alignleft actions">
                <input id="txtorderrecevepo" placeholder="Search Criteria" onkeypress="return AddKeyPressReceived(event);" autocomplete="off" class="form-control">
                <button type="button" id="btnreceveSearch" class="btn btn-primary btn-flat button">Search</button>
            </div>
        </div>
    </div>
</div>
