
@{
    ViewBag.Title = "ProfitLossAccountList";
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.rangePicker = "/Content/AdminLTE-3.1.0/bower_components/bootstrap-daterangepicker/daterangepicker.css";
}
<div id="loader" class="d-flex align-items-center">
    <img src="data:image/gif;base64," align="">
    <span>&nbsp;&nbsp;Loading......</span>
</div>
<div class="page-title">
    <h2> Profit Loss Account  </h2>
</div>
<section class="content section-content neworder-section table-design">
    <div class="panel">
        <div class="panel-body">
            <div class="row row mb-1">
                <div class="col-md-2">
                    <select class="form-control select2" id="ddlSearchBy" placeholder="Seach by" style="width: 100%;">
                        <option value="1" selected>Financial Year</option>
                        <option value="2">Custom Date</option>
                    </select>
                </div>
                <div class="col-md-4 search-control">
                    <select class="form-control select2" id="ddlfinaceyear">
                        <option value="0">Please Select Financial Year</option>
                    </select>
                </div>
                <div class="col-md-6">
                    <button type="button" title="Click here to print" data-placement="left" data-toggle="tooltip" id="btnSearch" class="btn btn-primary btn-flat button">Search</button>
                    <button type="button" title="Click here to print" data-placement="left" data-toggle="tooltip" id="btnChange" onclick="printDiv();" class="btn btn-primary btn-flat button pull-right">Print</button>
                </div>
            </div>
        </div>
    </div>

    <div id="printable" class="row">
        <div class="col-md-12 text-center">
            <strong>Layla Sleep Inc</strong>
        </div>
        <div class="col-md-12 text-center">
            <strong>Profit & Loss Account</strong>
        </div>
        <div class="col-md-12 text-center">
            <strong><span id="selectyear">-</span></strong>
        </div>

        <div class="col-md-12">
            <table>
                <tr>
                    <td valign="top">
                        <table id="tblass" class="table-blue table table-bordered table-striped dataTable">
                            <thead>
                                <tr>
                                    <th class="text-center" style="width: 60%">Particulars (Expense)</th>
                                    <th class="text-right" style="width: 40%"></th>

                                </tr>
                            </thead>
                            <tbody id="account_ass"></tbody>
                            <tfoot id="account_assfoot"></tfoot>
                        </table>
                    </td>
                    <td valign="top">
                        <table id="tbllib" class="table-blue table table-bordered table-striped dataTable">
                            <thead>
                                <tr>
                                    <th class="text-center" style="width: 60%">Particulars (Income)</th>
                                    <th class="text-right" style="width: 40%"></th>
                                </tr>
                            </thead>
                            <tbody id="account_lib"></tbody>
                            <tfoot id="account_libfoot"></tfoot>
                        </table>
                    </td>
                </tr>
                <tr>
                    <td>
                        <table class="table-blue table table-bordered table-striped dataTable">
                            <tr>
                                <td valign="top" width="60%">
                                    <strong id="lblProfit">Net Profit :</strong>
                                </td>
                                <td valign="top" width="40%" align="right">
                                    <strong><span id="Totalprofit"></span></strong>
                                </td>
                            </tr>
                        </table>
                    </td>
                    <td>
                        <table class="table-blue table table-bordered table-striped dataTable">
                            <tr>
                                <td valign="top" width="60%">
                                    <strong id="lblLoss">Net Loss :</strong>
                                </td>
                                <td valign="top" width="40%" align="right">
                                    <strong><span id="TotalLoss"></span></strong>
                                </td>
                            </tr>
                        </table>
                    </td>
                </tr>
                <tr>
                    <td valign="top">
                        <table class="table-blue table table-bordered table-striped dataTable">
                            <tr>
                                <td valign="top" width="60%">
                                    <strong>Total (Expense) :</strong>
                                </td>
                                <td valign="top" width="40%" align="right">
                                    <strong><span id="TotalExpense"></span></strong>
                                </td>
                            </tr>
                        </table>
                    </td>
                    <td valign="top">
                        <table class="table-blue table table-bordered table-striped dataTable">
                            <tr>
                                <td valign="top" width="60%">
                                    <strong>Total (Income) :</strong>
                                </td>
                                <td valign="top" width="40%" align="right">
                                    <strong><span id="TotalIncome"></span></strong>
                                </td>
                            </tr>
                        </table>
                        @*<strong>Total(Income) : <span id="TotalIncome"></span></strong>*@
                    </td>
                </tr>
            </table>
        </div>

    </div>
    <script type="text/javascript">
        $(document).ready(function () {
            $("#loader").hide(); $('.select2').select2();
            $.when(getfinaceyear()).done(function () { getProfitLoss() });
            $(document).on("change", "#ddlSearchBy", function (t) {
                t.preventDefault(); let id = parseInt($("#ddlSearchBy").val()) || 0; SearchByControl(id)
            });
            $('#ddlfinaceyear').change(function () { getProfitLoss(); });
            $(document).on("click", "#btnSearch", function (t) { t.preventDefault(); getProfitLoss(); });
        });
        ///Search Control
        function SearchByControl(id) {
            if (id == 1) {
                $(".search-control").empty().append('<select class="form-control select2" id="ddlfinaceyear" placeholder="Filter by Financial Year" style="width: 100%;"><option value="0">Please Select Financial Year</option></select>');
                $("#loader").show();
                $.when(getfinaceyear()).done(function () { $(".select2").select2(); getProfitLoss() });
            }
            else if (id == 2) {
                $(".search-control").empty().append('<div class="input-group"><span class= "input-group-addon"><i class="fa fa-calendar"></i></span ><input id="txtDate" data-auto-update-input="false" readonly="readonly" class="form-control"></div>');
                $('#txtDate').daterangepicker({
                    ranges: {
                        'Today': [moment(), moment()],
                        'Yesterday': [moment().subtract(1, 'days'), moment().subtract(1, 'days')],
                        'Last 7 Days': [moment().subtract(6, 'days'), moment()],
                        'Last 30 Days': [moment().subtract(29, 'days'), moment()],
                        'This Month': [moment().startOf('month'), moment().endOf('month')],
                        'Last Month': [moment().subtract(1, 'month').startOf('month'), moment().subtract(1, 'month').endOf('month')],
                        'This Year': [moment().subtract(1, 'year').startOf('month'), moment().subtract(1, 'year').endOf('month')],
                        'Last Year': [moment().startOf('year'), moment()]
                    },
                    startDate: moment().startOf('year'), autoUpdateInput: true, alwaysShowCalendars: true,
                    locale: { format: 'MM/DD/YYYY', cancelLabel: 'Clear' }, opens: 'right', orientation: "right auto"
                }, function (start, end, label) { getProfitLoss(); });
                $("#loader").show(); getProfitLoss();
            }
        }
        function getfinaceyear() {
            $.ajax({
                url: "/Accounting/Getfinancialyear", type: "Get",
                success: function (data) {
                    $('#ddlfinaceyear').empty().append('<option value="">Please Select Financial Year</option>');
                    $.each(data, function (i, row) {
                        $('#ddlfinaceyear').append('<option value="' + row.Value + '" ' + (row.Selected ? 'selected' : '') + '>' + row.Text + '</option>');
                    });
                }, async: false
            });
        }
        function formatCurrency(total) {
            var neg = false;
            if (total < 0) { neg = true; total = Math.abs(total); }
            return (neg ? "-$" : '$') + parseFloat(total, 10).toFixed(2).replace(/(\d)(?=(\d{3})+\.)/g, "$1,").toString();
        }
        function groupBy(xs, f) {
            return xs.reduce((r, v, i, a, k = f(v)) => ((r[k] || (r[k] = [])).push(v), r), {});
        }
        function getProfitLoss() {
            let _income = 0, _expense = 0;
            let _searchby = parseInt($('#ddlSearchBy').val()) || 0, _date_from = moment(), _date_to = moment();
            let option = {};
            if (_searchby == 2) {
                _date_from = $('#txtDate').data('daterangepicker').startDate, _date_to = $('#txtDate').data('daterangepicker').endDate;
                option = { fiscalyear_id: 0, from_date: _date_from.format('MM-DD-YYYY'), to_date: _date_to.format('MM-DD-YYYY'), report_type: 'PLREPORT' };
            }
            else {
                option = { fiscalyear_id: parseInt($('#ddlfinaceyear').val()) || 0, from_date: '', to_date: '', report_type: 'PLREPORT' };
            }
            $.ajax({
                url: "/accounting/get-trialbalance", dataType: 'json', contentType: "application/json; charset=utf-8", data: option, beforeSend: function () { $("#loader").show(); },
                success: function (result) {
                    try {
                        result = JSON.parse(result);
                        let _list = groupBy(result, (c) => c.extraparams);
                        let _inc_html = '', _exp_html = '';
                        $.each(_list, function (g_i, g_row) {
                            _date_from = moment(result[0].date_start), _date_to = moment(result[0].date_end)
                            let _sublist = groupBy(g_row, (c) => c.pcg_type);
                            $.each(_sublist, function (sg_i, sg_row) {
                                let _total = 0;
                                if (g_i == 'E') {
                                    _exp_html += '<tr id="tritemIdlib_' + sg_i + '" data-key="' + sg_i + '">';
                                    _exp_html += '<td colspan="2"><strong>' + sg_i + '</strong></td>';
                                    //itemHtmllib += '<td style="text-align:right;"><strong>' + formatCurrency(data['incnm'][i].Total) + '</strong></td>';
                                    _exp_html += '</tr>';
                                    $.each(sg_row, function (i, row) {
                                        _exp_html += '<tr id="tri_' + row.account_number + '" data-key="' + row.account_number + '">';
                                        _exp_html += '<td style="padding-left: 30px!important;">' + row.label + '</td>';
                                        _exp_html += '<td style="text-align:right;font-style: italic;">' + formatCurrency(row.invtotal) + '</td>';
                                        _exp_html += '</tr>';
                                        _total += parseFloat(row.invtotal) || 0;
                                    });
                                    _exp_html += '<tr id="tritemIdlib_' + sg_i + '" data-key="' + sg_i + '">';
                                    _exp_html += '<td><strong>Total - ' + sg_i + '</strong></td>';
                                    _exp_html += '<td style="text-align:right;"><strong>' + formatCurrency(_total) + '</strong></td>';
                                    _exp_html += '</tr>';
                                    _expense += _total;
                                }
                                else if (g_i == 'I') {
                                    if (sg_i != '-') {
                                        _inc_html += '<tr id="tritemIdlib_' + sg_i + '" data-key="' + sg_i + '">';
                                        _inc_html += '<td colspan="2"><strong>' + sg_i + '</strong></td>';
                                        //itemHtmllib += '<td style="text-align:right;"><strong>' + formatCurrency(data['incnm'][i].Total) + '</strong></td>';
                                        _inc_html += '</tr>';
                                        $.each(sg_row, function (i, row) {
                                            _inc_html += '<tr id="tri_' + row.account_number + '" data-key="' + row.account_number + '">';
                                            _inc_html += '<td style="padding-left: 30px!important;">' + row.label + '</td>';
                                            _inc_html += '<td style="text-align:right;font-style: italic;">' + formatCurrency(row.invtotal) + '</td>';
                                            _inc_html += '</tr>';
                                            _total += parseFloat(row.invtotal) || 0;
                                        });
                                        _inc_html += '<tr id="tritemIdlib_' + sg_i + '" data-key="' + sg_i + '">';
                                        _inc_html += '<td><strong>Total - ' + sg_i + '</strong></td>';
                                        _inc_html += '<td style="text-align:right;"><strong>' + formatCurrency(_total) + '</strong></td>';
                                        _inc_html += '</tr>';
                                    }
                                    else {
                                        $.each(sg_row, function (i, row) {
                                            _inc_html += '<tr id="tri_' + row.account_number + '" data-key="' + row.account_number + '">';
                                            _inc_html += '<td><strong>' + row.label + '</strong></td>';
                                            _inc_html += '<td style="text-align:right;"><strong>' + formatCurrency(row.invtotal) + '</strong></td>';
                                            _inc_html += '</tr>';
                                            _total += parseFloat(row.invtotal) || 0;
                                        });
                                    }
                                    _income += _total;
                                }
                            });
                        });
                        $('#account_ass').empty().append(_exp_html);
                        $('#account_lib').empty().append(_inc_html);
                    }
                    catch (error) { throw error; }
                },
                complete: function () { $("#loader").hide(); },
                error: function (xhr, status, err) { $("#loader").hide(); swal('Alert!', "something went wrong.", "error"); }, async: true
            });
            let _profit = _income - _expense;
            if (_profit >= 0) {
                $("#TotalIncome").html(formatCurrency(_income));
                $("#lblProfit").html('Net Profit  :'); $("#Totalprofit").html(formatCurrency(_profit));
                $("#lblLoss,#TotalLoss").html('-');
                $("#TotalExpense").html(formatCurrency(_expense + _profit));
            }
            else if (_profit < 0) {
                $("#lblLoss").html(formatCurrency('Net Loss :')); $("#TotalLoss").html(formatCurrency((-1 * _profit)));
                $("#TotalIncome").html(formatCurrency(_income + (-1 * _profit)));
                $("#lblProfit,#Totalprofit").html('-');
                $("#TotalExpense").html(formatCurrency(_expense));
            }

            $("#selectyear").html(_date_from.format('MMMM, DD YYYY') + ' to ' + _date_to.format('MMMM, DD YYYY'));
        }
        function printDiv() {
            // var newWin = window.open('', 'Print-Window', 'width=400,height=400,top=100,left=100');
            var divName = 'printable';
            var printContents = document.getElementById(divName).innerHTML;
            var originalContents = document.body.innerHTML;
            document.body.innerHTML = printContents;
            //let title_el = document.querySelector("title");
            //title_el.innerHTML = "World";
            document.title = ".";

            window.print();
            // newWin.document.title = "Readings on PageLinks";
            document.body.innerHTML = originalContents;

            getfinaceyear();
            //$('#txtOrderDate').daterangepicker({
            //    ranges: {
            //        'Today': [moment(), moment()],
            //        'Yesterday': [moment().subtract(1, 'days'), moment().subtract(1, 'days')],
            //        'Last 7 Days': [moment().subtract(6, 'days'), moment()],
            //        'Last 30 Days': [moment().subtract(29, 'days'), moment()],
            //        'This Month': [moment().startOf('month'), moment().endOf('month')],
            //        'Last Month': [moment().subtract(1, 'month').startOf('month'), moment().subtract(1, 'month').endOf('month')]
            //    },
            //    startDate: moment().subtract(1, 'month'), autoUpdateInput: true, alwaysShowCalendars: true,
            //    locale: { format: 'MM/DD/YYYY', cancelLabel: 'Clear' }, opens: 'right', orientation: "left auto"
            //}, function (start, end, label) {
            //    getAssets(true);
            //});
        }
    </script>
</section>
