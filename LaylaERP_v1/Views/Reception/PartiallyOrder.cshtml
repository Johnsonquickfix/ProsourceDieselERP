            <div class="col-md-12">
                <div class="box box-primary">
                    <div class="box-header">
                        <div class="row">
                            <div class="col-sm-12">
                                <table id="dtdataPartially" class="inventory-table table-blue table check-table table-bordered table-striped dataTable1" width="100%">
                                    <thead>
                                        <tr>
                                            @*<th width="5%" style="display:none"><input type="checkbox" name="checkall" id="checkall" onClick="CheckAll();"><label></label></th>*@
                                            <th>PO No.</th>
