
@{
    ViewBag.Title = "Customer Service (Help Desk)";
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.rangePicker = "/Content/AdminLTE-3.1.0/bower_components/bootstrap-daterangepicker/daterangepicker.css";
    var user = LaylaERP.UTILITIES.CommanUtilities.Provider.GetCurrent();
}
<link href="~/Content/AdminLTE-3.1.0/dropzone/dropzone.min.css" rel="stylesheet" />
<link href="~/css/style.bundle.css" rel="stylesheet" />
<!-- datatables CSS -->
<link href="~/Content/AdminLTE-3.1.0/datatables/DataTables-1.10.20/css/dataTables.bootstrap4.min.css" rel="stylesheet" />
<link href="~/Content/AdminLTE-3.1.0/datatables/Buttons-1.6.1/css/buttons.dataTables.min.css" rel="stylesheet" />
<!-- datatables js -->
<script src="~/Content/AdminLTE-3.1.0/datatables/datatables.min.js" type="text/javascript"></script>
<script src="~/Content/AdminLTE-3.1.0/datatables/DataTables-1.10.20/js/dataTables.bootstrap4.min.js" type="text/javascript"></script>
<style>
    .swal2-container .swal2-popup {
        max-width: 500px;
    }

    body .swal2-popup .swal2-styled {
        background-color: rgb(48, 133, 214) !important;
        border-left-color: rgb(48, 133, 214) !important;
        border-right-color: rgb(48, 133, 214) !important;
    }

    .float-right {
        float: right !important;
    }

    .timeline:before {
        background: none !important;
    }

    .form-switch.form-check-solid .form-check-input {
        background-image: none !important;
    }
</style>
<div id="loader" class="align-items-center">
    <img src="data:image/gif;base64," align="">
    <span>&nbsp;&nbsp;Loading......</span>
</div>
<!--begin::Page title-->
<div class="page-title">
    <h2 class="page-heading" data-paypal_seller="@user.PaypalSellerAccount">Customer Service (Help Desk)</h2>
    <div class="active text-right" style="margin-left:auto;min-width:280px;">
    </div>
</div>
<!--end::Page title-->
<section id="list-page" class="content mb-5">
    <div class="card card-flush mb-5">
        <div class="card-header lh-1">
            <div class="card-title">
                <h2>Search </h2>
            </div>
        </div>
        <div class="card-body p-lg-7 pt-lg-0">
            <div class="row mb-1">
                <div class="col-md-4">
                    <select class="form-control select2" id="ddlSearchBy" placeholder="Seach by" style="width: 100%;">
                        <option value="1">Search by Email</option>
                        <option value="2">Search by Customer</option>
                        <option value="3" selected>Search by Order No.</option>
                        <option value="4">Search by Phone No.</option>
                    </select>
                </div>
                <div class="col-md-4 search-control">
                    <input id="txtOrderNo" class="form-control" placeholder="Order No." style="width: 100%;" maxlength="10">
                    <!--<select class="form-control select2" id="ddlEmail" placeholder="Filter by Billing Email" style="width: 100%;">
                    </select>-->
                </div>
                <div class="col-md-4">
                    <a id="btnSearch" class="btn btn-danger btn-sm btn-flat" data-toggle="tooltip" title="Search Customer Details with Orders."><i class="fa fa-search"></i> Filter</a>
                </div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-md-3">
            <div class="card card-flush">
                <div class="card-body pt-0 p-10">
                    <h3 class="profile-username text-center">-</h3>
                    <p class="profile-useremail text-muted text-center">-</p>
                    <hr />
                    <strong><i class="fas fa-map-marker-alt mr-1"></i> Billing Address</strong>
                    <p class="text-muted billing-address">-</p>
                    <hr />
                    <strong><i class="fas fa-map-marker-alt mr-1"></i> Shipping Address</strong>
                    <p class="text-muted shipping-address">-</p>
                    <ul class="list-group list-group-unbordered mb-3">
                        <li class="list-group-item">
                            <b>Orders</b> <span class="float-right a-text-bold total-orders">0</span>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
        <div class="col-md-9">
            <div class="card">
                <div class="card-body">
                    <table id="dtdata" class="table dataTable table-row-dashed no-footer text-propercase text-active-gray-700" width="100%">
                    </table>
                </div>
            </div>
        </div>
    </div>
</section>
<div id="detail-page" class="content p-0 mb-lg-5 hidden">
    <div class="card mb-5">
        <!--begin::Body-->
        <div class="card-body p-lg-7">
            <!--begin::Layout-->
            <div class="d-flex flex-xl-row">
                <!--flex-column-->
                <!--begin::Sidebar-->
                <div class="m-0 me-xl-10 me-lg-5">
                    <!--begin::Invoice 2 sidebar-->
                    <div class="d-print-none border border-dashed border-gray-300 card-rounded h-lg-100 min-w-md-300px p-5 bg-lighten">
                        <!--begin::Labels-->
                        <div class="mb-2 float-right">
                            <span class="badge badge-light-success me-2 order-status">-</span>
                            <!--<span class="badge badge-light-warning">Pending Payment</span>-->
                        </div>
                        <!--end::Labels-->
                        <!--begin::Title-->
                        <h6 class="mb-5 fw-boldest text-gray-600 text-hover-primary order-id">Order #000000</h6>
                        <!--end::Title-->
                        <!--begin::Item-->
                        <div class="mb-6">
                            <div class="fw-bold text-gray-600 fs-7">Order Date:</div>
                            <div class="fw-bolder text-gray-800 fs-6 order-date">-</div>
                        </div>
                        <!--end::Item-->
                        <!--begin::Item-->
                        <div class="mb-6">
                            <div class="fw-bold text-gray-600 fs-7">Billing:</div>
                            <div class="fw-bolder text-gray-800 fs-6 order-billing">-</div>
                        </div>
                        <!--end::Item-->
                        <!--begin::Item-->
                        <div class="mb-6">
                            <div class="fw-bold text-gray-600 fs-7">Shipping:</div>
                            <div class="fw-bolder text-gray-800 fs-6 order-shipping">-</div>
                        </div>
                        <!--end::Item-->
                        <!--begin::Item-->
                        <div class="mb-6">
                            <div class="fw-bold text-gray-600 fs-7">Order Amount:</div>
                            <div class="fw-bolder text-success fs-6 order-amount">-</div>
                        </div>
                        <!--end::Item-->
                        <!--begin::Item-->
                        <div class="mb-6">
                            <div class="fw-bold text-gray-600 fs-7">Order tax:</div>
                            <div class="fw-bolder text-gray-800 fs-6 order-tax">-</div>
                        </div>
                        <!--end::Item-->
                        <!--begin::Item-->
                        <div class="mb-6">
                            <div class="fw-bold text-gray-600 fs-7">Payment Mathod:</div>
                            <div class="fw-bolder text-gray-800 fs-6 order-payment">-</div>
                        </div>
                        <!--end::Item-->
                    </div>
                    <!--end::Invoice 2 sidebar-->
                </div>
                <!--end::Sidebar-->
                <!--begin::Content-->
                <div class="flex-lg-row-fluid mb-10 mb-xl-0">
                    <!--begin::Invoice 2 content-->
                    <div class="mt-n1">
                        <!--begin::Wrapper-->
                        <div class="m-0">
                            <!--begin::Label-->
                            <div class="d-flex flex-stack flex-grow-1">
                                <div class="fw-bolder fs-3 order-right-id">
                                    Order #000000
                                </div>
                                <div class="justify-content-end order-right-tool">
                                </div>
                            </div>
                            <!--end::Label-->
                            <!--begin::Content-->
                            <div class="flex-grow-1">
                                <!--begin::Table-->
                                <div class="table-responsive border-bottom mb-9">
                                    <table class="table align-middle table-row-dashed fs-6 gy-5 dataTable no-footer">
                                        <thead>
                                            <tr class="text-gray-700 fw-bolder text-muted">
                                                <th class="min-w-25px pb-2"></th>
                                                <th class="min-w-175px pb-2">Item</th>
                                                <th class="min-w-70px text-end pb-2">Sale Price</th>
                                                <th class="min-w-70px text-end pb-2">Quantity</th>
                                                <th class="min-w-80px text-end pb-2">Discount</th>
                                                <th class="min-w-80px text-end pb-2">Total</th>
                                                <th class="min-w-80px text-end pb-2">Tax</th>
                                            </tr>
                                        </thead>
                                        <tbody id="order_items"></tbody>
                                        <tbody id="order_items_refund"></tbody>
                                    </table>
                                </div>
                                <!--end::Table-->
                                <!--begin::Container-->
                                <div class="row">
                                    <div class="col-md-6 px-6 pb-5 order-coupon">
                                    </div>
                                    <!--begin::Section-->
                                    <div class="col-md-5 col-md-offset-1">
                                        <div class="border border-dashed border-gray-300 bg-lighten card-rounded p-6" id="order-footer">
                                            <!--begin::Item-->
                                            <div class="d-flex flex-stack">
                                                <!--begin::Code-->
                                                <div class="fw-bold pe-10 text-gray-600 fs-7">Total</div>
                                                <!--end::Code-->
                                                <!--begin::Label-->
                                                <div class="text-end fw-bolder fs-6 text-gray-800">$0.00</div>
                                                <!--end::Label-->
                                            </div>
                                            <!--end::Item-->
                                        </div>
                                    </div>
                                    <!--end::Section-->
                                </div>
                                <!--end::Container-->
                            </div>
                            <!--end::Content-->
                        </div>
                        <!--end::Wrapper-->
                    </div>
                    <!--end::Invoice 2 content-->
                </div>
                <!--end::Content-->

            </div>
            <!--end::Layout-->
        </div>
        <!--end::Body-->
    </div>
    <div class="row gy-5 g-xl-10">
        <div class="col-md-6 mb-xl-10">
            <div class="card">
                <div class="card-header min-h-60px">
                    <!--begin::Title-->
                    <h3 class="card-title align-items-start flex-column">
                        <span class="card-label fw-bolder text-gray-800 order-id-comment">Order #000000 Comments</span>
                    </h3>
                    <!--end::Title-->
                </div>
                <!--begin::Body-->
                <div class="card-body p-lg-10">
                    <!--begin::Layout-->
                    <div class="d-flex flex-column flex-xl-row">
                        <!--begin::Timeline-->
                        <div class="timeline ms-n1 mb-0 order-notes">
                        </div>
                        <!--end::Timeline-->
                    </div>
                    <!--end::Layout-->
                </div>
                <!--end::Body-->
            </div>
        </div>
        <div class="col-md-6 mb-xl-10">
            <div class="card">
                <div class="card-header min-h-60px">
                    <!--begin::Title-->
                    <h3 class="card-title align-items-start flex-column">
                        <span class="card-label fw-bolder text-gray-800 order-id-tickets">Order Tickets</span>
                    </h3>
                    <!--end::Title-->
                </div>
                <!--begin::Body-->
                <div class="card-body p-lg-10">
                    <!--begin::Layout-->
                    <div class="d-flex flex-column flex-xl-row">
                        <!--begin::Timeline-->
                        <div class="timeline ms-n1 mb-0 order-tickets">
                        </div>
                        <!--end::Timeline-->
                    </div>
                    <!--end::Layout-->
                </div>
                <!--end::Body-->
            </div>
        </div>
    </div>
</div>
<div class="modal in printable" id="myModal" role="dialog" aria-hidden="true"></div>

<script src="~/js/serializePHP.js"></script>
<script src="~/js/common.js"></script>

<script src="~/Content/AdminLTE-3.1.0/dropzone/dropzone.min.js"></script>
<script src="@Url.Content("~/js/Orders/OrderCoupons.js?v=" + DateTime.Now.ToString("ddMMyyyymmss"))" asp-append-version="true"></script>
<script src="@Url.Content("~/js/CustomerService/WarrantyQuestions.js?v="+DateTime.Now.ToString("ddMMyyyymmss")+"")" asp-append-version="true"></script>
<script src="@Url.Content("~/js/CustomerService/CustomerSearch.js?v="+DateTime.Now.ToString("ddMMyyyymmss")+"")" asp-append-version="true"></script>

